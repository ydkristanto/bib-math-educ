@article{ WOS:000404604700016,
Author = {Duan, Qingying},
Title = {A Study of the Influence of Learning Organization on Organizational
   Creativity and Organizational Communication in High Tech Technology},
Journal = {EURASIA JOURNAL OF MATHEMATICS SCIENCE AND TECHNOLOGY EDUCATION},
Year = {2017},
