@article{ WOS:000354825400006,
Author = {Ekawati, Rooselyna and Lin, Fou-Lai and Yang, Kai-Lin},
Title = {Primary Teachers' knowledge for teaching ratio and proportion in
   Mathematics: The case of Indonesia},
