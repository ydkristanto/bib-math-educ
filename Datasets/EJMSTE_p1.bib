@article{ WOS:000409067500015,
Author = {Yang, Chia-Lee and Huang, Chi-Yo and Kao, Yu-Sheng and Tasi, Yi-Lang},
Title = {Disaster Recovery Site Evaluations and Selections for Information
   Systems of Academic Big Data},
Journal = {EURASIA JOURNAL OF MATHEMATICS SCIENCE AND TECHNOLOGY EDUCATION},
