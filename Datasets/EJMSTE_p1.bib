@article{ WOS:000404607800030,
Author = {Kaymakamoglu, Sibel Ersel},
Title = {Science Teachers' Conceptualizations and Implications for the
   Development of the Professional Development Programs},
Journal = {EURASIA JOURNAL OF MATHEMATICS SCIENCE AND TECHNOLOGY EDUCATION},
Year = {2017},
