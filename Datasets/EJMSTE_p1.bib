@article{ WOS:000417628000045,
Author = {Yin, Hang and Zhang, Longquan and Zhang, Yuhan},
Title = {The Effect of Ownership Concentration and Related Diversification
   Strategy of Entrepreneurial Enterprises on Enterprise Performance},
Journal = {EURASIA JOURNAL OF MATHEMATICS SCIENCE AND TECHNOLOGY EDUCATION},
Year = {2017},
Volume = {13},
Number = {12},
Pages = {8073-8087},
Month = {DEC},
Abstract = {Based on the theory of principal-agent theory, this paper makes an
   empirical study on the process of influencing the performance of
   enterprises by using the analysis method of
   ``power-decision-performance{''} analysis, using hierarchical regression
   and group regression analysis to analyze the diversification strategy of
   entrepreneurial enterprises analysis. The results show that the
   ownership concentration of entrepreneurial enterprises has a positive
   impact on the financial performance of entrepreneurial enterprises and
   has a negative impact on the market performance of entrepreneurial
   enterprises. The increase of ownership concentration will inhibit the
   diversified development tendency of entrepreneurial enterprises, and the
   related diversification strategy which has a positive effect on the
   financial performance and market performance of the enterprise. The
   related diversification strategy has some mediating effect in the
   process of the impact of ownership concentration on the performance of
   the firm, and the negative report of the media has a negative adjustment
   in this process.},
