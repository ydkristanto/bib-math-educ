@article{ WOS:000404603400005,
Author = {Lin, Jing},
Title = {Chinese Grade Eight Students' Understanding About the Concept of Global
   Warming},
Journal = {EURASIA JOURNAL OF MATHEMATICS SCIENCE AND TECHNOLOGY EDUCATION},
