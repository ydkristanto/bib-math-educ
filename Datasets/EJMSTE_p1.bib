@article{ WOS:000423586100038,
Author = {Faize, Fayyaz Ahmad and Husain, Waqar and Nisar, Farhat},
Title = {A Critical Review of Scientific Argumentation in Science Education},
Journal = {EURASIA JOURNAL OF MATHEMATICS SCIENCE AND TECHNOLOGY EDUCATION},
Year = {2018},
Volume = {14},
Number = {1},
Pages = {475-483},
Month = {JAN},
Abstract = {The use of argumentation in science education is associated with many
   benefits. Some of these include developing critical skills, promoting
   spirit of enquiry, enhancing conceptual understanding and improving
   academic performance of students. However, there are also some issues
   and challenges while using argumentation in science classrooms. This
   research will discuss the strength of using scientific argumentation in
   science education. The findings from other such studies will also be
   critically reviewed to seek an in-depth understanding of the use of
   argumentation in teaching and associated challenges. The research would
   help in improving the use of argumentation in teaching and exploring
   solution to problems and challenges associated with this method.},
