@article{ WOS:000404607800050,
Author = {Chen, Zheng and Miller, Patrick A. and Clements, Terry L. and Kim,
   Mintai},
Title = {Mapping Research in Landscape Architecture: Balancing Supply of Academic
   Knowledge and Demand of Professional Practice},
