@article{ WOS:000404604700010,
Author = {John, Merlin and Molepo, Jacob Maisha and Chirwa, Max},
Title = {South African Learners' Conceptual Understanding about Image Formation
   by Lenses},
Journal = {EURASIA JOURNAL OF MATHEMATICS SCIENCE AND TECHNOLOGY EDUCATION},
