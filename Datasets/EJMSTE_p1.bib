@article{ WOS:000404604700025,
Author = {Battaglia, Onofrio Rosario and Di Paola, Benedetto and Fazio, Claudio},
Title = {K-means Clustering to Study How Student Reasoning Lines Can Be Modified
   by a Learning Activity Based on Feynman's Unifying Approach},
Journal = {EURASIA JOURNAL OF MATHEMATICS SCIENCE AND TECHNOLOGY EDUCATION},
Year = {2017},
Volume = {13},
