@article{ WOS:000404603400016,
Author = {Bermudez Hernandez, Jonathan and Chalela, Salim and Valencia Arias,
   Jackeline and Valencia Arias, Alejandro},
Title = {Research Trends in the Study of ICT Based Learning Communities: A
   Bibliometric Analysis},
