Title = {Methodological Teaching System of Mathematical Foundations of Formal
   Languages as a Means of Fundamentalization of Education},
Journal = {EURASIA JOURNAL OF MATHEMATICS SCIENCE AND TECHNOLOGY EDUCATION},
