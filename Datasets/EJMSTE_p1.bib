@article{ WOS:000404607800024,
Author = {Hsieh, Ming-Yuan},
Title = {An Empirical Study of Education Divide Diminishment through Online
   Learning Courses},
Journal = {EURASIA JOURNAL OF MATHEMATICS SCIENCE AND TECHNOLOGY EDUCATION},
