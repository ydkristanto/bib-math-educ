@article{ WOS:000404591100020,
Author = {Liu, Po-Hung},
Title = {Sociocultural Perspectives on the Internationalization of Research in
   Mathematics Education: A Survey Based on JRME, ESM, and MTL},
Journal = {EURASIA JOURNAL OF MATHEMATICS SCIENCE AND TECHNOLOGY EDUCATION},
