@article{ WOS:000352005600007,
Author = {Liu, Hong-Cheng and Yen, Jih-Rong},
Title = {Effects of Distance Learning on Learning Effectiveness},
Journal = {EURASIA JOURNAL OF MATHEMATICS SCIENCE AND TECHNOLOGY EDUCATION},
Year = {2014},
Volume = {10},
Number = {6},
Pages = {575-580},
Month = {DEC},
Abstract = {The development of computers in the past two decades has resulted in the
   changes of education in enterprises and schools. The advance of computer
   hardware and platforms allow colleges generally applying distance
   courses to instruction that both Ministry of Education and colleges have
   paid attention to the development of Distance Learning. To improve the
