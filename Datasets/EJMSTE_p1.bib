@article{ WOS:000329446400008,
Author = {Shieh, Chich-Jen and Liao, Ying and Hu, Ridong},
Title = {Web-Based Instruction, Learning Effectiveness and Learning Behavior: The
   Impact of Relatedness},
Journal = {EURASIA JOURNAL OF MATHEMATICS SCIENCE AND TECHNOLOGY EDUCATION},
Year = {2013},
Volume = {9},
Number = {4},
Pages = {405-410},
Month = {NOV},
Abstract = {This study aims to discuss the effects of Web-based Instruction and
   Learning Behavior on Learning Effectiveness. Web-based Instruction
   contains the dimensions of Active Learning, Simulation-based Learning,
   Interactive Learning, and Accumulative Learning; and, Learning Behavior
   covers Learning Approach, Learning Habit, and Learning Attitude. The
   directors of Web-based Instruction Centers and the students in ten
   universities in Taiwan and China are selected as the research subjects
   for on-site questionnaire distribution and collection. Within the 500
   copies distributed, total 382 valid ones are retrieved, with the
   retrieval rate 76\%. The research findings show the partially
   significant correlations between Web-based Instruction and Learning
   Behavior, between Learning Behavior and Learning Effectiveness, and
   between Web-based Instruction and Learning Effectiveness. At the end,
   several suggestions are proposed for the practice of Web-based
   Instruction.},
