@article{ WOS:000369554700015,
Author = {Catma, Zehra and Corlu, Mehmet Sencer},
Title = {How Special are Teachers of Specialized Schools? Assessing
   Self-Confidence Levels in the Technology Domain},
Journal = {EURASIA JOURNAL OF MATHEMATICS SCIENCE AND TECHNOLOGY EDUCATION},
