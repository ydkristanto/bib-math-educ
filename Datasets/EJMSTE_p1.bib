@article{ WOS:000335382100003,
Author = {Elena Escalera-Chavez, Milka and Garcia-Santillan, Arturo and
   Venegas-Martinez, Francisco},
Title = {Modeling Attitude toward Statistics by a Structural Equation},
Journal = {EURASIA JOURNAL OF MATHEMATICS SCIENCE AND TECHNOLOGY EDUCATION},
Year = {2014},
Volume = {10},
Number = {1},
Pages = {23-31},
Month = {FEB},
Abstract = {In this study, we examined whether the constructs of usefulness,
   motivation, likeness, confidence, and anxiety influence the student's
   attitude towards statistics. Two hundred ninety eight students enrolled
   in the private university were surveyed by using the questionnaire
   proposed by Auzmendi (1992). Data analysis was done by structural
   equation model AMOS software. The results support the model proposed by
   Auzmendi of five factors, however the data reveal that there exists an
   alternative model (CFI = 0.907) that best fits the proposed model (CFI =
   0.885). Furthermore, of the 25 indicators proposed only 22 have an
   acceptable range and two of the indicators (Items 9 and 2) should be
   considered in the construct of anxiety and usefulness respectively.},
