@article{ WOS:000341620900004,
Author = {Kulgemeyer, Christoph and Schecker, Horst},
Title = {Research on Educational Standards in German Science Education Towards a
   model of student competences},
Journal = {EURASIA JOURNAL OF MATHEMATICS SCIENCE AND TECHNOLOGY EDUCATION},
