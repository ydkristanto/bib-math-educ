@article{ WOS:000663521000001,
Author = {Maass, Katja and Sorge, Stefan and Romero-Ariza, Marta and Hesse, Alice
   and Straser, Oliver},
Title = {Promoting Active Citizenship in Mathematics and Science Teaching},
