@article{ WOS:000368636900009,
Author = {Lin, Shu-Hui and Huang, Yun-Chen},
Title = {DEVELOPMENT AND APPLICATION OF A CHINESE VERSION OF THE SHORT ATTITUDES
   TOWARD MATHEMATICS INVENTORY},
