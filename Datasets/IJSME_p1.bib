@article{ WOS:000324644600007,
Author = {Hsieh, Feng-Jui},
Title = {STRENGTHENING THE CONCEPTUALIZATION OF MATHEMATICS PEDAGOGICAL CONTENT
   KNOWLEDGE FOR INTERNATIONAL STUDIES: A TAIWANESE PERSPECTIVE},
