@article{ WOS:000352657000007,
Author = {Kaiser, Gabriele and Busse, Andreas and Hoth, Jessica and Koenig,
   Johannes and Bloemeke, Sigrid},
Title = {About the Complexities of Video-Based Assessments: Theoretical and
   Methodological Approaches to Overcoming Shortcomings of Research on
   Teachers' Competence},
Journal = {INTERNATIONAL JOURNAL OF SCIENCE AND MATHEMATICS EDUCATION},
Year = {2015},
