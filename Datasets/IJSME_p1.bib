@article{ WOS:000292149200006,
Author = {Tabach, Michal and Barkai, Ruthi and Tsamir, Pessia and Tirosh, Dina and
   Dreyfus, Tommy and Levenson, Esther},
Title = {VERBAL JUSTIFICATION-IS IT A PROOF? SECONDARY SCHOOL TEACHERS'
   PERCEPTIONS},
Journal = {INTERNATIONAL JOURNAL OF SCIENCE AND MATHEMATICS EDUCATION},
Year = {2010},
Volume = {8},
Number = {6},
Pages = {1071-1090},
Month = {DEC},
Abstract = {According to reform documents, teachers are expected to teach proofs and
   proving in school mathematics. Research results indicate that high
   school students prefer verbal proofs to other formats. We found it
   interesting and important to examine the position of secondary school
   teachers with regard to verbal proofs. Fifty high school teachers were
   asked to prove various elementary number theory statements, to write
   correct and incorrect proofs that students may use, and to evaluate
   given justifications to statements from elementary number theory. While
   all the participants provided correct proofs to the statements, our
   findings indicate that teachers are not aware of students' preference
   for verbal justifications. Also, about half of the teachers rejected
   correct verbal justifications. They claimed that these justifications
   lacked generality and are mere examples.},
