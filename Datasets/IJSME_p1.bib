@article{ WOS:000529465200002,
Author = {Yow, Jan A. and Wilkerson, Ashlye and Gay, Celeste},
Title = {Mathematics and Science Teacher Leadership Understanding Through a
   Teacher Leadership Course},
Journal = {INTERNATIONAL JOURNAL OF SCIENCE AND MATHEMATICS EDUCATION},
Year = {2021},
Volume = {19},
Number = {4},
Pages = {839-862},
Month = {APR},
Abstract = {This article shares findings from a study conducted in the southeastern
   United States of the impact of a teacher leadership course on upper
   elementary, middle, and high school mathematics and science teachers.
   Fifty-nine teachers completed the course and data analysis showed growth
   in understanding in five areas of teacher leadership. This growth was
   analyzed using a four-image teacher leadership conceptual framework.
   Data analysis revealed a fifth image of growth that more fully
   represented the teacher leadership development of the participating
   teachers. Details of specific teacher leader characteristics within each
   image are included and findings are presented addressing growth within
   each of the five images based on the course experience. Discussion is
   offered about the implications for mathematics and science teacher
   leader development derived from the data analysis.},
