@article{ WOS:000334682200003,
Author = {Seybold, Brigitte and Braunbeck, Thomas and Randler, Christoph},
Title = {PRIMATE CONSERVATION-AN EVALUATION OF TWO DIFFERENT EDUCATIONAL PROGRAMS
   IN GERMANY},
