@article{ WOS:000722786200001,
Author = {Ko, Yeonjoo and Shim, Sungok Serena and Lee, Hyunju},
Title = {Development and Validation of a Scale to Measure Views of Social
   Responsibility of Scientists and Engineers (VSRoSE)},
