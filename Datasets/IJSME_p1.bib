@article{ WOS:000577576800001,
Author = {Ayalon, Michal and Naftaliev, Elena and Levenson, Esther S. and Levy,
   Sigal},
Title = {Prospective and In-Service Mathematics Teachers' Attention to a Rich
   Mathematics Task While Planning its Implementation in the Classroom},
Journal = {INTERNATIONAL JOURNAL OF SCIENCE AND MATHEMATICS EDUCATION},
Year = {2021},
Volume = {19},
Number = {8},
Pages = {1695-1716},
Month = {DEC},
Abstract = {This study explores secondary mathematics teachers' attention to the
   three components of Jaworski's (1992) ``teaching triad{''}
   (i.e.,mathematical challenge,learning management, andsensitivity to
   students) as they planned a lesson revolving around a rich mathematics
   problem and assessed pre-designed student solutions for that problem.
   Written work was gathered from 17 cohorts. Qualitative analysis
   generated categories. Quantitative analysis revealed that some
   components of the teaching triad were attended to in both activities,
   some were not attended to in either activity, and some were attended to
   in one activity but not in the other. Findings are interpreted in light
   of theory.},
