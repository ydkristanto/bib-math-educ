@article{ WOS:000427491000007,
Author = {Olsson, Jan},
Title = {The Contribution of Reasoning to the Utilization of Feedback from
   Software When Solving Mathematical Problems},
Journal = {INTERNATIONAL JOURNAL OF SCIENCE AND MATHEMATICS EDUCATION},
Year = {2018},
Volume = {16},
Number = {4},
Pages = {715-735},
Month = {APR},
Abstract = {This study investigates how students' reasoning contributes to their
   utilization of computer-generated feedback. Sixteen 16-year-old students
   solved a linear function task designed to present a challenge to them
   using dynamic software, GeoGebra, for assistance. The data were analysed
   with respect both to character of reasoning and to the use of feedback
   generated through activities in GeoGebra. The results showed that
   students who successfully solved the task were engaged in creative
   reasoning and used feedback extensively.},
