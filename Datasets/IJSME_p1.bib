@article{ WOS:000538359500003,
Author = {Smith, Thomas J. and Walker, David A. and Chen, Hsiang-Ting and Hong,
   Zuway-R},
Title = {Students' Sense of School Belonging and Attitude towards Science: a
   Cross-Cultural Examination},
Journal = {INTERNATIONAL JOURNAL OF SCIENCE AND MATHEMATICS EDUCATION},
Year = {2020},
Volume = {18},
Number = {5},
Pages = {855-867},
Month = {JUN},
Abstract = {The present study examined data from the 2015 Trends in International
   Mathematics and Science Study to address cross-cultural differences in
   4th grade students' attitudes towards science and how students' sense of
   school belonging (SSSB) scores were related to attitude towards science.
   Regression results indicated that SSSB emerged as a significant,
   positive predictor of Students Like Learning Science (SLS) and Students
   Confident in Science (SCS) scale scores across countries. Additionally,
   statistically significant moderating effects for Taiwan were evident for
   the relationship between SSSB and both SLS and SCS scores. Specifically,
   when compared with students in the USA, the positive effect of SSSB on
   these attitudinal outcomes was less pronounced among students in Taiwan.},
