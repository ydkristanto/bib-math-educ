@article{ WOS:000446177300010,
Author = {Tran, Dung and Tarr, James E.},
Title = {Examination of Bivariate Data Tasks in US High School Textbooks Through
   the Statistical Investigation and Cognitive Demands Frameworks},
Journal = {INTERNATIONAL JOURNAL OF SCIENCE AND MATHEMATICS EDUCATION},
