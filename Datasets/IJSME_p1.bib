@article{ WOS:000365728500014,
Author = {Stein, Hana and Galili, Igal},
Title = {THE IMPACT OF AN OPERATIONAL DEFINITION OF THE WEIGHT CONCEPT ON
   STUDENTS' UNDERSTANDING},
Journal = {INTERNATIONAL JOURNAL OF SCIENCE AND MATHEMATICS EDUCATION},
