@article{ WOS:000207961500008,
Author = {Kaberman, Zvia and Dori, Yehudit Judy},
Title = {QUESTION POSING, INQUIRY, AND MODELING SKILLS OF CHEMISTRY STUDENTS IN
   THE CASE-BASED COMPUTERIZED LABORATORY ENVIRONMENT},
Journal = {INTERNATIONAL JOURNAL OF SCIENCE AND MATHEMATICS EDUCATION},
