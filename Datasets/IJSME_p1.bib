@article{ WOS:000852810500001,
Author = {Kolovou, Maria and Ran, Hua and Secada, Walter},
Title = {CGI Teachers' Patterns of Interacting with Male and Female Students
   During Their First and Second Years of Practice},
Journal = {INTERNATIONAL JOURNAL OF SCIENCE AND MATHEMATICS EDUCATION},
