@article{ WOS:000715734200001,
Author = {Yim, Jaehoon and Lee, Hyung Sook},
Title = {Students' Use of Prior Conceptions of Symbols in Finding an Equation for
   a Horizontal Translation of the Graph of a Function},
Journal = {INTERNATIONAL JOURNAL OF SCIENCE AND MATHEMATICS EDUCATION},
