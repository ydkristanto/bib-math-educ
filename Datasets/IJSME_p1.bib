@article{ WOS:000796307000001,
Author = {Jiang, Ronghuan and Star, Jon R. and Hasto, Peter and Li, Lijia and Liu,
   Ru-de and Tuomela, Dimitri and Prieto, Nuria Joglar and Palkki, Riikka
   and Abanades, Miguel A. and Pejlare, Johanna},
Title = {Which One Is the ``Best{''}: a Cross-national Comparative Study of
   Students' Strategy Evaluation in Equation Solving},
Journal = {INTERNATIONAL JOURNAL OF SCIENCE AND MATHEMATICS EDUCATION},
Year = {2023},
Volume = {21},
