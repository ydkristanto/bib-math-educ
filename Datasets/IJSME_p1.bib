@article{ WOS:000368636900007,
Author = {Paige, Kathryn and Zeegers, Yvonne and Lloyd, David and Roetman, Philip},
Title = {RESEARCHING THE EFFECTIVENESS OF A SCIENCE PROFESSIONAL LEARNING
   PROGRAMME USING A PROPOSED CURRICULUM FRAMEWORK FOR SCHOOLS: A CASE
   STUDY},
Journal = {INTERNATIONAL JOURNAL OF SCIENCE AND MATHEMATICS EDUCATION},
