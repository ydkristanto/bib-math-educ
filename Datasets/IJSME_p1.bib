Title = {Providing Support and Examples for Teaching Linear Equations in
   Secondary School: the Role of Knowledge of Mathematics Teaching},
Journal = {INTERNATIONAL JOURNAL OF SCIENCE AND MATHEMATICS EDUCATION},
Year = {2023},
