@article{ WOS:000522578200001,
Author = {Schoenfeld, Alan H.},
Title = {On Meaningful, Researchable, and Generative Questions},
Journal = {INTERNATIONAL JOURNAL OF SCIENCE AND MATHEMATICS EDUCATION},
