Affiliation = {Godino, JD (Corresponding Author), Univ Granada, E-18071 Granada, Spain.
   Godino, Juan D.; Ake, Lila P.; Gonzato, M., Univ Granada, E-18071 Granada, Spain.
   Wilhelmi, Miguel R., Univ Publ Navarra, Navarra, Spain.},
DOI = {10.5565/rev/ensciencias.965},
