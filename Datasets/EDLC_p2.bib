@article{ WOS:000543377600010,
Author = {Miranda, Isaias and Freire Gard, Elena Glenda},
Title = {An innovative proposal for the inclusion of moral values in mathematics
   teaching},
Journal = {ENSENANZA DE LAS CIENCIAS},
Year = {2020},
Volume = {38},
Number = {1},
Pages = {183-196},
Abstract = {The aim of this article is twofold: 1) to document the design of an
   innovative teaching sequence that links problems related to the concept
   of piecewise function with questions involving moral dilemmas; 2) to
   analyze how middle school students (17-18 years old) establish a
   relationship between the mathematical concept and specific moral values
   when responding to the dilemmas. The methodology used was drawn on
   considerations of a socio-formal approach to the learner. The analysis
   was made on the basis of students' written and spoken discourse during
   the classroom. Results indicate that, in responding to dilemmas,
   students manage to manipulate the concept of piecewise function not only
   in terms of its mathematical meaning, but also of its moral value. We
   conclude that the didactic sequence may help to organize a more
   comprehensive teaching for mathematics students.},
