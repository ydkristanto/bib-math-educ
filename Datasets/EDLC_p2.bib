@article{ WOS:001180238700003,
Author = {Cebrian, Gisela and Moraleda, Alvaro and Olano, Jon Xavier and Boque,
   Anna and Prieto, Juan},
Title = {Secondary Education Students' Sustainability Competencies According to
   Gender},
Journal = {ENSENANZA DE LAS CIENCIAS},
Year = {2024},
Volume = {42},
Number = {1},
Abstract = {center dot In the last decades, a growing recognition of the importance
   of integrating sustainability competences at different educational
   levels is plausible; however, to date studies on secondary education are
   still scarce. The aim of this descriptive study is to explore the self
   -perception of the development of sustainability competences in
   secondary education students and its relationship with gender. A
   questionnaire was administered to 372 students from 5 secondary
   education schools in Tarragona (Spain). The results show a good level of
   self -perception of sustainability competences, with significant
   differences between men and women, mainly in attitudes and behaviors in
   sustainability. This study can inform the design of curricula and the
   development of educational strategies for sustainability.},
