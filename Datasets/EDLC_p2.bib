@article{ WOS:000427015900009,
Author = {Fernandez, Ceneida and Sanchez-Matamoros, Gloria and Moreno, Mar and Luz
   Callejo, Maria},
Title = {<i>Coordination of approximations</i> in the understanding of the limit
   concept when prospective teachers anticipate students' answers},
Journal = {ENSENANZA DE LAS CIENCIAS},
Year = {2018},
Volume = {36},
Number = {1},
Pages = {143-162},
Abstract = {The aim of this study is to examine to what extent the identification of
   the coordination of approximations in the domain and in the range is
   considered a Key Developmental Understanding (KDU in Simon, 2006) by
   prospective teachers. 25 prospective secondary school mathematics
   teachers anticipated high school students' answers to problems about the
   limit of a function showing different characteristics of understanding
   and proposed problems that would support students' conceptual progress.
   Results show that identifying the coordination of the approximations in
   the domain and in the range as KDU allowed prospective teachers to
   consider progressions in students' learning in order to support their
   teaching decisions.},
