@article{ WOS:000339986400005,
Author = {Mordeglia, Cecilia and Mengascini, Adriana},
Title = {Characterization of experimental practices in school based on primary
   and secondary teachers speech},
Journal = {ENSENANZA DE LAS CIENCIAS},
Year = {2014},
Volume = {32},
Number = {2},
Pages = {71-89},
Month = {JUN},
Abstract = {We present results of a questionnaire aiming at the characterization of
   teaching practices in natural sciences, particularly those involving
   experimental approaches. It was answered by teachers at different levels
   of the education system within the province of Buenos Aires, Argentina.
   The survey outlined a starting point of a process of joint reflection on
   teaching practice. It was found that experimental activities are rarely
   or not performed. This is attributed to limitations with institutional,
   personal and curriculum origins. The mentioned and selected tasks and
   the purposes of such activities refer to objectives of conceptual
   knowledge and reflect an image of empirical science, with a strong
   emphasis on observation.},
