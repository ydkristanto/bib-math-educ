@article{ WOS:000246055200003,
Author = {Jacobs, Victoria R. and Franke, Megan Loef and Carpenter, Thomas P. and
   Levi, Linda and Battey, Dan},
Title = {Professional development focused on children's algebraic reasoning in
   elementary school},
