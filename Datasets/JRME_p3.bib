@article{ WOS:A1993ME09000003,
Author = {CARPENTER, TP and ANSELL, E and FRANKE, ML and FENNEMA, E and WEISBECK,
   L},
Title = {MODELS OF PROBLEM-SOLVING - A STUDY OF KINDERGARTEN CHILDRENS
   PROBLEM-SOLVING PROCESSES},
Journal = {JOURNAL FOR RESEARCH IN MATHEMATICS EDUCATION},
Year = {1993},
Volume = {24},
Number = {5},
Pages = {428-441},
Month = {NOV},
Abstract = {Seventy kindergarten children who had spent the year solving a variety
   of basic word problems were individually interviewed as they solved
   addition, subtraction, multiplication, division, multistep, and
   nonroutine word problems. Thirty-two children used a valid strategy for
   all nine problems and 44 correctly answered seven or more problems. Only
   5 children were not able to answer any problems correctly. The results
   suggest that children can solve a wide range of problems, including
   problems involving multiplication and division situations, much earlier
   than generally has been presumed. With only a few exceptions, children's
   strategies could be characterized as representing or modeling the action
   or relationships described in the problems. The conception of problem
   solving as modeling could provide a unifying framework for thinking
   about problem solving in the primary grades. Modeling offers a
   parsimonious and coherent way of thinking about children's mathematical
   problem solving that is relatively straightforward and is accessible to
   teachers and students alike.},
