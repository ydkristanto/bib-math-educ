@article{ WOS:000917069900002,
Author = {Schukajlow, S. and Rakoczy, K. and Pekrun, R.},
Title = {Emotions and motivation in mathematics education: Where we are today and
   where we need to go},
Journal = {ZDM-MATHEMATICS EDUCATION},
Year = {2023},
