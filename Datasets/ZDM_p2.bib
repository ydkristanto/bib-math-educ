@article{ WOS:000608380100001,
Author = {Dominguez, Higinio},
Title = {Fraction detectives: bilingual students investigate the hidden
   identities of equivalent fractions},
Journal = {ZDM-MATHEMATICS EDUCATION},
Year = {2021},
