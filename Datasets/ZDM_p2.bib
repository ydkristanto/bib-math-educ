@article{ WOS:000434287500011,
Author = {Walkington, Candace and Marder, Michael},
Title = {Using the UTeach Observation Protocol (UTOP) to understand the quality
   of mathematics instruction},
Journal = {ZDM-MATHEMATICS EDUCATION},
