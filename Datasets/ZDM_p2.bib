@article{ WOS:000401722700010,
Author = {Cambria, Jenna and Brandt, Holger and Nagengast, Benjamin and Trautwein,
   Ulrich},
Title = {Frame of Reference effects on values in mathematics: evidence from
   German secondary school students},
Journal = {ZDM-MATHEMATICS EDUCATION},
