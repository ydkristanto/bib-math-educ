@article{ WOS:000516080000001,
Author = {Hollebrands, Karen F. and Lee, Hollylynne S.},
Title = {Effective design of massive open online courses for mathematics teachers
   to support their professional learning},
Journal = {ZDM-MATHEMATICS EDUCATION},
