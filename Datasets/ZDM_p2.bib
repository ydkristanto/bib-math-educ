@article{ WOS:001020231400001,
Author = {van der Wal, Nathalie J. J. and Bakker, Arthur and Drijvers, Paul},
Title = {Designing an instrument to measure the development of
   techno-mathematical literacies in an innovative mathematics course for
   future engineers in STEM education},
