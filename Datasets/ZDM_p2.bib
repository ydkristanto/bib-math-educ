@article{ WOS:000559845800001,
Author = {Dawkins, Paul Christian},
Title = {Identifying aspects of mathematical epistemology that might influence
   productively student reasoning beyond mathematics},
Journal = {ZDM-MATHEMATICS EDUCATION},
