@article{ WOS:000378472000002,
Author = {Lewis, Katherine E. and Fisher, Marie B.},
Title = {Taking Stock of 40 Years of Research on Mathematical Learning
   Disability: Methodological Issues and Future Directions},
Journal = {JOURNAL FOR RESEARCH IN MATHEMATICS EDUCATION},
Year = {2016},
Volume = {47},
Number = {4},
Pages = {338-371},
Month = {JUL},
Abstract = {Although approximately 5-8\% of students have a mathematical learning
   disability (MLD), researchers have yet to develop a consensus
   operational definition. To examine how MLD has been identified and what
   mathematics topics have been explored, we conducted a systematic review
   of 164 studies on MLD published between 1974 and 2013. Findings indicate
   that (a) there was great variability in the classification methods used,
   (b) studies rarely reported demographic differences between the MLD and
   typically achieving groups, and (c) studies overwhelmingly focused on
   elementary-aged students engaged in basic arithmetic calculation. To
   move the field toward a more precise and shared definition of MLD, we
   argue for standards for methodology and reporting, and we identify a
   need for research addressing more complex mathematics.},
