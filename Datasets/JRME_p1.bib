@article{ WOS:000177812600002,
Author = {Hoyles, C and Noss, R and Pozzi, S},
Title = {Proportional reasoning in nursing practice},
Journal = {JOURNAL FOR RESEARCH IN MATHEMATICS EDUCATION},
Year = {2001},
Volume = {32},
Number = {1},
Pages = {4-27},
Month = {JAN},
Abstract = {We investigate how expert nurses undertake the calculation of drug
   dosages on the ward, This calculation is error-critical in nursing
   practice and maps onto the concepts of ratio and proportion. Using
   episodes of actual drug administration gleaned from ethnographic study,
   we provide evidence that experienced nurses use a range of correct
   proportional-reasoning strategies based on the invariant of drug
   concentration to calculate dosage on the ward instead of the single
   taught method they describe outside of the practice. These strategies
   are fled to individual drugs, specific quantities and volumes of drugs,
   the way drugs are packaged, and the organization of clinical work.},
