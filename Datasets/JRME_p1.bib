@article{ WOS:000071574500002,
Author = {Carpenter, TP and Franke, ML and Jacobs, VR and Fennema, E and Empson,
   SB},
Title = {A longitudinal study of invention and understanding in children's
   multidigit addition and subtraction},
Journal = {JOURNAL FOR RESEARCH IN MATHEMATICS EDUCATION},
Year = {1998},
Volume = {29},
Number = {1},
Pages = {3-20},
Month = {JAN},
Note = {Annual Meeting of the American-Educational-Research-Association, NEW
   YORK, NEW YORK, APR 08-12, 1996},
Organization = {Amer Educ Res Assoc},
Abstract = {This 3-year longitudinal study investigated the development of 82
   children's understanding of multidigit number concepts and operations in
   Grades 1-3. Students were individually interviewed 5 times on a variety
   of tasks involving base-ten number concepts and addition and subtraction
   problems. The study provides an existence proof that children can invent
   strategies for adding and subtracting and illustrates both what that
   invention affords and the role that different concepts may play in that
   invention. About 90\% of the students used invented strategies. Students
   who used invented strategies before they learned standard algorithms
   demonstrated better knowledge of base-ten number concepts and were more
   successful in extending their knowledge to new situations than were
   students who initially learned standard algorithms.},
