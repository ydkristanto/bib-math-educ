Affiliation = {Mulligan, JT (Corresponding Author), MACQUARIE UNIV,SCH EDUC,N RYDE,NSW 2109,AUSTRALIA.},
DOI = {10.2307/749783},
ISSN = {0021-8251},
Keywords-Plus = {EXPONENTIAL FUNCTIONS; WORD-PROBLEMS},
