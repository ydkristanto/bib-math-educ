@article{ WOS:000400223400002,
Author = {Langer-Osuna, Jennifer M.},
Title = {Authority, Identity, and Collaborative Mathematics},
Journal = {JOURNAL FOR RESEARCH IN MATHEMATICS EDUCATION},
Year = {2017},
Volume = {48},
Number = {3},
Pages = {237-247},
Month = {MAY},
Abstract = {The field of mathematics education research has seen a resurgence of
   interest in understanding collaborative learning because students in
   K-12 classrooms are increasingly expected to make sense of mathematics
   problems together. This research commentary argues for the importance of
   understanding student authority relations in collaborative mathematics
   classrooms. How intellectual authority becomes constructed, organized,
   and distributed among students has implications for both mathematics
   learning and the development of mathematics-linked identities. This
   research commentary suggests directions for future work to gain clarity
   on the mechanisms that undergird the distribution of authority in order
   to support powerful mathematics classrooms.},
