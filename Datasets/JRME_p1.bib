@article{ WOS:000367280200003,
Author = {Hohensee, Charles},
Title = {Teachers' Awareness of the Relationship Between Prior Knowledge and New
   Learning},
Journal = {JOURNAL FOR RESEARCH IN MATHEMATICS EDUCATION},
Year = {2016},
Volume = {47},
Number = {1},
Pages = {17-27},
Month = {JAN},
Abstract = {In this study, I examined the degree to which experienced teachers were
   aware of the relationship between prior knowledge and new learning.
   Interviews with teachers revealed that they were explicitly aware of
   when students made connections between prior knowledge and new learning,
   when they applied their prior knowledge to new contexts, and when they
   developed their prior knowledge as a result of applying that knowledge
   to new contexts. However, teachers were not explicitly aware of
   backward-transfer effects. Results from this study have implications for
   future research on backward transfer, as well as for teacher
   professional development.},
