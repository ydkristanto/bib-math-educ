@article{ WOS:000465187500003,
Author = {Harper, Frances K.},
Title = {A Qualitative Metasynthesis of Teaching Mathematics for Social Justice
   in Action: Pitfalls and Promises of Practice},
Journal = {JOURNAL FOR RESEARCH IN MATHEMATICS EDUCATION},
Year = {2019},
Volume = {50},
Number = {3},
Pages = {268-310},
Month = {MAY},
Abstract = {Mathematics classrooms are increasingly becoming sites for investigating
   social (in)justice, but research on teaching mathematics for social
   justice remains limited to individual case studies. This article reports
   on a metasynthesis of 35 qualitative reports of social justice
   mathematics enactments in diverse classroom contexts. Critical race
   theory serves as a guiding framework for analyzing possibilities and
   limitations of these enactments to address racial inequities in
   mathematics education. Findings from this metasynthesis reveal that
   addressing race in social justice mathematics explorations provided
   opportunities for centering the voices of people of Color and critiquing
   liberal views that camouflage subtle forms of racism and involved
   substantial and authentic mathematical work. Promising practices and
   implications for future research are identified based on this synthesis.},
Publisher = {NATL COUNCIL TEACHERS MATHEMATICS-NCTM},
