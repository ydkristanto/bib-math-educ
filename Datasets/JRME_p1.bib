@article{ WOS:000956603000001,
Author = {Herbst, Patricio and Chazan, Daniel and Matthews, Percival G. and
   Lichtenstein, Erin K. and Crespo, Sandra},
Title = {How Manuscripts Can Contribute to Research on Mathematics Education:
   Possibilities for Applied Research},
Journal = {JOURNAL FOR RESEARCH IN MATHEMATICS EDUCATION},
Year = {2023},
Volume = {54},
Number = {1},
Pages = {2-6},
Month = {JAN},
Abstract = {In our editorial last January, Herbst et al. (2022) echoed a question
   often raised by reviewers of manuscripts: What is this manuscript's
   contribution to our research field? In that first elaboration on how
   manuscripts may contribute to the field of research in mathematics
   education, we discussed the contributions of basic research. In this
   editorial, motivated by the illustrations provided by the articles
   included in this issue, we do a similar exercise with applied research.
   We argue that, beyond answering the specific questions posed, applied
   research studies in mathematics education can contribute to our field by
   offering benchmark conceptualizations of the systems in which
   research-generated knowledge is usable or by identifying aspects of
   these systems for which better theory or measures may be needed.},
