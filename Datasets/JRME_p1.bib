@article{ WOS:000071574500005,
Author = {Atweh, B and Bleicher, RE and Cooper, TJ},
Title = {The construction of the social context of mathematics classrooms: A
   sociolinguistic analysis},
Journal = {JOURNAL FOR RESEARCH IN MATHEMATICS EDUCATION},
Year = {1998},
Volume = {29},
Number = {1},
Pages = {63-82},
Month = {JAN},
Abstract = {This study employed sociolinguistic perspectives developed by Michael
   Halliday to investigate the social context of 2 mathematics classrooms
   that differed in the socioeconomic backgrounds and genders of their
   students. Our analysis focused on the effects of these 2 factors on
   teacher perceptions of student needs and abilities and on how these
   perceptions shaped the discourse in the classroom. We argue that as
   mathematical knowledge is being constructed in the classroom, the
   student participants are being constructed by the teachers according to
   their ability in and need for different dialects of mathematics that
   have different values in our Western culture.},
