@article{ WOS:000262092700004,
Author = {Cobb, Paul and Gresalfi, Melissa and Hodge, Lynn Liao},
Title = {An Interpretive Scheme for Analyzing the Identities That Students
   Develop in Mathematics Classrooms},
Journal = {JOURNAL FOR RESEARCH IN MATHEMATICS EDUCATION},
Year = {2009},
Volume = {40},
Number = {1},
Pages = {40-68},
Month = {JAN},
Abstract = {Our primary purpose in this article is to propose an interpretive scheme
   for analyzing the identities that students develop in mathematics
   classrooms that can inform instructional design and teaching. We first
   introduce the key constructs of normative identity and personal
   identity, and then illustrate how they can be used to conduct empirical
   analyses. The case on which the sample analysis focuses concerns a
   single group of middle school students who were members of two
   contrasting classrooms in which what it meant to know and do mathematics
   differed significantly. The resulting analyses document the forms of
   agency that students can legitimately exercise in particular classrooms,
   together with how authority is distributed and thus to whom Students are
   accountable, and what they are accountable for mathematically. In the
   final section of the article, we clarify the relation of the
   interpretive scheme to other current work on the identities that
   students are developing in mathematics classrooms.},
