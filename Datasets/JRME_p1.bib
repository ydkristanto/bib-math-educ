@article{ WOS:000319708900012,
Author = {Meaney, Tamsin and Trinick, Tony and Fairhall, Uenuku},
Title = {One Size Does NOT Fit All: Achieving Equity in Maori Mathematics
   Classrooms},
Journal = {JOURNAL FOR RESEARCH IN MATHEMATICS EDUCATION},
Year = {2013},
Volume = {44},
Number = {1},
Pages = {235-263},
Month = {JAN},
Abstract = {In this article, we explore how a school in Aotearoa {[}New Zealand]
   infuses the identity of Indigenous students into the school-based
   curriculum through the promotion of their language and culture in
   mathematics lessons. Bernstein's pedagogic device illustrates how
   teachers' practices were influenced by being able to think the
   ``unthinkable.{''} This came from the contestation that arose when
   competing bodies of knowledge had to be integrated both at the school
   level and at the classroom level. For equity to be achieved regarding
   students' mathematics learning;parents' and the community's aspirations
   for students' education need to be infused into debates about the
   knowledge that teachers are expected to include in their teaching. This
   enables the local context to make a positive contribution to students'
   learning. It also implies that programs for improvement should not be
   imposed on schools unless there are opportunities for them to be adapted
   to the needs of individual schools.},
Publisher = {NATL COUNCIL TEACHERS MATHEMATICS-NCTM},
Address = {1906 ASSOCIATION DRIVE, RESTON, VA 22091 USA},
