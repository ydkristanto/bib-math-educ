@article{ WOS:000316307300004,
Author = {Steffe, Leslie P.},
Title = {Establishing Mathematics Education as an Academic Field: A Constructive
   Odyssey},
Journal = {JOURNAL FOR RESEARCH IN MATHEMATICS EDUCATION},
Year = {2013},
Volume = {44},
Number = {2},
Pages = {354-370},
Month = {MAR},
Abstract = {Through my work in mathematics education, I have come to the realization
   that constituting mathematics education as an academic field entails
