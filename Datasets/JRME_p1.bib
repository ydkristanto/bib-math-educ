@article{ WOS:000208620300001,
Author = {Bray, Wendy S.},
Title = {A Collective Case Study of the Influence of Teachers' Beliefs and
   Knowledge on Error-Handling Practices During Class Discussion of
   Mathematics},
Journal = {JOURNAL FOR RESEARCH IN MATHEMATICS EDUCATION},
Year = {2011},
Volume = {42},
Number = {1},
Pages = {2-38},
Month = {JAN},
Abstract = {This collective case study examines the influence of 4 third-grade
   teachers' beliefs and knowledge on their error-handling practices during
   class discussion of mathematics. Across cases, 3 dimensions of teachers'
   error-handling practices are identified and discussed in relation to
   teacher beliefs and knowledge: (a) intentional focus on flawed solutions
   in class discussion, (b) promotion of conceptual understanding through
   discussion of errors, and (c) mobilization of a community of learners to
   address errors. Study findings suggest that, although teachers' ways of
   handling student errors during class discussion of mathematics are
   clearly linked to both teacher beliefs and teacher knowledge, some
   aspects of teacher response are more strongly linked to knowledge and
   others are influenced more by beliefs.},
