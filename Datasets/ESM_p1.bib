@article{ WOS:000349368500003,
Author = {Garfield, Joan and Le, Laura and Zieffler, Andrew and Ben-Zvi, Dani},
Title = {Developing students' reasoning about samples and sampling variability as
   a path to expert statistical thinking},
Journal = {EDUCATIONAL STUDIES IN MATHEMATICS},
Year = {2015},
Volume = {88},
Number = {3, SI},
Pages = {327-342},
Month = {MAR},
Abstract = {This paper describes the importance of developing students' reasoning
   about samples and sampling variability as a foundation for statistical
   thinking. Research on expert-novice thinking as well as statistical
   thinking is reviewed and compared. A case is made that statistical
   thinking is a type of expert thinking, and as such, research comparing
   novice and expert thinking can inform the research on developing
   statistical thinking in students. It is also posited that developing
   students' informal inferential reasoning, akin to novice thinking, can
   help build the foundations of experts' statistical thinking.},
