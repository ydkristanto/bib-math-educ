@article{ WOS:000544842100001,
Author = {Radford, Luis},
Title = {Davydov's concept of the concept and its dialectical materialist
   background},
Journal = {EDUCATIONAL STUDIES IN MATHEMATICS},
Year = {2021},
Volume = {106},
Number = {3, SI},
Pages = {327-342},
Month = {MAR},
Abstract = {The goal of this article is to investigate Davydov's concept of the
   concept against the backdrop of its philosophical system, namely,
   dialectical materialism. In the first part, after briefly sketching the
   context of Davydov's work, I consider some ontological and
   epistemological ideas on which Davydov bases his concept of the concept.
   I pay particular attention to Hegel's and Marx's contributions. Then, I
   discuss Davydov's concept of the concept and the relationship between
   thelogicaland thehistorical-a relationship that proved to be crucial in
   the making of the educational curricular program he and El'konin
   launched in the 1960s in Russia. I argue that, in tune with the dominant
   epistemology of the twentieth century, Davydov's concept of the concept
   is based on a scientific outlook of the world, one in which theoretical
   scientific thought is considered the pinnacle of human cognition. I
   conclude with a critique that intends to place the notion of the concept
   in a broader dialectical materialist perspective.},
