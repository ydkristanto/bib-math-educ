@article{ WOS:000207960300004,
Author = {Lagrange, Jean-Baptiste and Erdogan, Emel Ozdemir},
Title = {Teachers' emergent goals in spreadsheet-based lessons: analyzing the
   complexity of technology integration},
Journal = {EDUCATIONAL STUDIES IN MATHEMATICS},
