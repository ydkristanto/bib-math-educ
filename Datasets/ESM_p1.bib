   Kaisari, Maria; Patronis, Tasos, Univ Patras, Dept Math, GR-26110 Patras, Greece.},
DOI = {10.1007/s10649-010-9255-4},
ISSN = {0013-1954},
EISSN = {1573-0816},
Keywords = {Elliptic geometry; Construction of models; Interaction; Negotiation of
   meaning; Mathematical practice},
