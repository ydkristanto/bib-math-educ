@article{ WOS:000637453300001,
Author = {Bakker, Arthur and Cai, Jinfa and Zenger, Linda},
Title = {Future themes of mathematics education research: an international survey
   before and during the pandemic},
Journal = {EDUCATIONAL STUDIES IN MATHEMATICS},
