@article{ WOS:000329645800005,
Author = {Hong, Dae S. and Choi, Kyong Mi},
Title = {A comparison of Korean and American secondary school textbooks: the case
   of quadratic equations},
Journal = {EDUCATIONAL STUDIES IN MATHEMATICS},
Year = {2014},
Volume = {85},
Number = {2},
Pages = {241-263},
Month = {FEB},
Abstract = {This study compares quadratic equations sections of Korean and American
   textbooks. The number of topics, contents and mathematics items were
   analyzed. The results show Korean students learn some topics relatively
   earlier than American students. American textbooks include more problems
   requiring explanations and various representations and problems
   requiring higher level cognitive demand. This result could indicate that
   textbooks might not be the reason for American and Korean students'
   performances in international comparative studies. More studies
   analyzing other standards based textbooks and teaching practice might be
   needed to have further understanding of mathematics education in two
   countries.},
