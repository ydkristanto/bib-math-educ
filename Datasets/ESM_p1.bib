@article{ WOS:000413956700005,
Author = {Guillemette, David},
Title = {History of mathematics in secondary school teachers' training: towards a
   nonviolent mathematics education},
Journal = {EDUCATIONAL STUDIES IN MATHEMATICS},
Year = {2017},
Volume = {96},
Number = {3},
Pages = {349-365},
Month = {NOV},
Abstract = {In the context of mathematics teachers' training, the concept of
   d,paysement ,pist,mologique (epistemological disorientation) emphasizes
   that the contact with the history of mathematics, particularly with the
   use of original sources, pushes aside commonplace students' perspectives
   about the discipline and offers them a critical look towards
   mathematics's historical, social and cultural aspects. Conceptually
   supported by the theory of objectivation, an emergent sociocultural
   theory in mathematics education, this study describes the d,paysement
   ,pist,mologique lived by future mathematics teachers engaged in the
   reading of historical texts. A phenomenological approach allowed us to
   clarify various meanings associated with students' lived experiences and
   a dialogical perspective provides a way to get these meanings in tension
   through a polyphonic narration. Our reading of this polyphonic narration
   suggests that d,paysement ,pist,mologique associated with the reading of
   historical texts encouraged empathy from students towards the authors
   and their future learners, opening up the possibility for a nonviolent
   mathematics education.},
