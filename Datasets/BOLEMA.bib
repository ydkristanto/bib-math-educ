@article{ WOS:000272623300002,
Author = {Cury, Helena Noronha and Bisognin, Eleni},
Title = {Analysis of Solutions to a Problem Represented by a System of Equations},
Journal = {BOLEMA-MATHEMATICS EDUCATION BULLETIN-BOLETIM DE EDUCACAO MATEMATICA},
Year = {2009},
Volume = {22},
Number = {33},
Pages = {1-21},
Abstract = {Large-scale assessment projects of students' performance in Brazil use
   descriptors to indicate abilities that students should develop in order
   to deal with real-life modeling problems, and one of these abilities is
   related to solving systems of equations. In this paper, we report on
   part of a research project developed with first-year students in
   mathematics courses enrolled in private universities in southern Brazil;
   one test question was chosen for an indepth analysis of written
   solutions to a system of linear equations. After classifying the work of
   the students, ideas about the meanings of symbol and structure were used
   to discuss difficulties that arose. It is considered that such an
   analysis can clarify aspects that must be the focus of future studies to
   help students and teachers to understand some problems presented in
   large-scale assessments.},
