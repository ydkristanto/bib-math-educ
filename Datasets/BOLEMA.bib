@article{ WOS:000308802200002,
Author = {Bovo, Audria Alessandra and Ferrari Gasparotto, Giovana Cristina and
   Rotondo, Margareth Sacramento and Carrera de Souza, Antonio Carlos},
Title = {Researching Practices and Tactics in Mathematics Education},
