@article{ WOS:000306844900004,
Author = {Rosa Corica, Ana and Rita Otero, Maria},
Title = {Study of the Praxeologies Proposed to Study in a University Calculus
   Course},
Journal = {BOLEMA-MATHEMATICS EDUCATION BULLETIN-BOLETIM DE EDUCACAO MATEMATICA},
Year = {2012},
Volume = {26},
Number = {42B},
Pages = {459-482},
Abstract = {In this research we analyzed the organizations proposed to be studied in
   a university calculus course relative to the notions of functional limit
   and continuity. The material developed by teachers for the university
   students was analyzed using the Anthropologic Theory of the Didactic.
   The principal results indicate that the materials propose the study of
   isolated tasks, which do not lead to the production and validation of
   technological elements. Thus, knowledge appears to be organized in two
   levels: one theoretical and other practical, where the latter does not
   affect the conformation of the former. This generates an inadequate
   interpretation of scientific knowledge, reducing its study to
   disconnected mathematical organizations.},
