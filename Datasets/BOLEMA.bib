@article{ WOS:000272623100007,
Author = {Moreira, Plinio Cavalcanti and Costa Ferreira, Maria Cristina},
Title = {The Theory of Subconstructs and the Rational Number as Operator: from
   algebraic to cognitive structures},
Journal = {BOLEMA-MATHEMATICS EDUCATION BULLETIN-BOLETIM DE EDUCACAO MATEMATICA},
Year = {2008},
Volume = {21},
Number = {31},
Pages = {103-127},
Abstract = {We present a literature review on the role of the operator subconstruct
   in the learning of rational numbers. We chose to focus on this
   subconstruct because it is related to the operation of multiplication,
   which places it in a position of importance when one considers the
   development of so-called multiplicative reasoning. We reviewed the
