@article{ WOS:000284098100009,
Author = {Cavalcanti, Milka Rossana G. and Natrielli, Karla Renata B. and
   Guimaraes, Gilda Lisboa},
Title = {Printed Media Graphs},
Journal = {BOLEMA-MATHEMATICS EDUCATION BULLETIN-BOLETIM DE EDUCACAO MATEMATICA},
Year = {2010},
Volume = {23},
Number = {36},
Pages = {733-751},
Abstract = {This study aimed to analyze printed media graphs, considering three
   media: a daily newspaper, a weekly magazine and a monthly magazine. In
   theses medias a wide use of graphs were observed, sometimes in the same
   news report. Different relations between texts and graphs were observed:
   description and analysis of the graph in the text and graphs that were
   not part of the report, just maintaining connections with the news
   theme. It was observed that 6\% of the graphs analyzed presented the
   scale explicitly and 39\% of the scales presented proportionality
   mistakes. The present study reinforces the importance of teaching and
   learning this mathematical content due to its constant presence in
   people's daily life and of the needed understanding for the development
   of citizenship.},
