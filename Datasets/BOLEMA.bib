@article{ WOS:000272623100005,
Author = {Ferreira da Silva, Maria Jose and Almouloud, Saddo Ag},
Title = {Operations with Rational Numbers and their Meanings based on the
   Part-all Conception},
Journal = {BOLEMA-MATHEMATICS EDUCATION BULLETIN-BOLETIM DE EDUCACAO MATEMATICA},
Year = {2008},
Volume = {21},
Number = {31},
Pages = {55-78},
Abstract = {We present a reflection regarding operations with fractional numbers
   focusing on the part-all conception through some activities that can
   contribute to teaching practices at the elementary school level. A brief
   characterization of the all-part conception is provided along with
   descriptions of activities involving the four basic operations based on
   the use of flat figures, although other conceptions may emerge during
   the solution. We limit our considerations to this conception because it
   is the most frequent, in textbooks as well as teacher practice, although
   it is not used to justify operational rules of fractional numbers.},
