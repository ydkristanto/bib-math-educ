@article{ WOS:000306844300010,
Author = {Geromel Meneghetti, Renata Cristina and Priscila Redling, Julyette},
Title = {Alternative Tasks for the Teaching and the Learning of functions:
   analysis of an intervention in the High School},
Journal = {BOLEMA-MATHEMATICS EDUCATION BULLETIN-BOLETIM DE EDUCACAO MATEMATICA},
Year = {2012},
Volume = {26},
Number = {42A},
Pages = {193-229},
Abstract = {In this paper we focus on the application of two mathematical
   alternative tasks to the teaching and learning of functions with high
   school students. The tasks were elaborated according to the following
   methodological approach: (i) Problem Solving and/or mathematics
   investigation and (ii) a pedagogical proposal, which defends that
   mathematical knowledge is developed by means of a balance between logic
   and intuition. We employed a qualitative research approach
   (characterized as a case study) aimed at analyzing the didactic
   pedagogical potential of this type of methodology in high school. We
   found that tasks such as those presented and discussed in this paper
   provide a more significant learning for the students, allowing a better
