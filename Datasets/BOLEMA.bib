@article{ WOS:000274255400006,
Author = {Cordeiro Barroso, Natalia Maria and Soares, Jose Marques and Moura Mota,
   Joao Cesar and Borges Neto, Herminio},
Title = {Technological Instrumentation and Feedback in the Assessment Process in
   University-level Mathematics Teaching: a method based on Didactics
   Engineering},
Journal = {BOLEMA-MATHEMATICS EDUCATION BULLETIN-BOLETIM DE EDUCACAO MATEMATICA},
Year = {2009},
Volume = {22},
Number = {34},
Pages = {131-151},
Abstract = {With the use of automated tools, a change in paradigms in the student
   assessment process is necessary. This is due to the different mechanisms
   used to elaborate assessments and to analyze their results. An
   assessment method with continuous feedback is proposed. An application
   of this method is presented in which the activities proposed to the
   students in the classroom are based on Mathematical Didactic theories.
   To elaborate and apply this assessment, software was used that tracks
   the students' activities. Based on analysis of the teacher's behavior
   during the experiment, which showed his natural difficulty in modifying
   his teaching habits, it is emphasized that this type of educational work
   needs to involve the participation of professionals from the fields of
   mathematical education and technology to help improve the teacher's
   work.},
