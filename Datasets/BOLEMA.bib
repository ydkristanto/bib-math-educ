@article{ WOS:000274255400003,
Author = {Oliveira, Izabella},
Title = {Proportionality: problem-solving strategies used by Elementary School
   students in Quebec},
Journal = {BOLEMA-MATHEMATICS EDUCATION BULLETIN-BOLETIM DE EDUCACAO MATEMATICA},
Year = {2009},
Volume = {22},
Number = {34},
Pages = {57-79},
Abstract = {This study sought to determine the problem-solving strategies most used
   by 13- and 14-year-old 8(th) graders with regard to the concept of
   direct and inverse proportion. We also examined whether these students
   were able to recognize non-proportional contexts and specific challenges
   with regard to problem solving prior to learning the concept. Students
   from a Montreal, Quebec Elementary School participated in this study.
   Our analysis revealed the used of several problem-solving strategies.
   The results show the potential and the diversity of the strategies
   employed prior to learning the concept of proportions and some of the
   challenges related to this concept.},
