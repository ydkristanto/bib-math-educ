@article{ WOS:000869248200003,
Author = {Kang, Jingoo},
Title = {Gendered Science Practice at Secondary School and its Effects on Science
   Motivations},
Journal = {INTERNATIONAL JOURNAL OF SCIENCE AND MATHEMATICS EDUCATION},
