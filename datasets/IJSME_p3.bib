@article{ WOS:000713082200001,
Author = {Kim, Mijung},
Title = {Student Agency and Teacher Authority in Inquiry-Based Classrooms: Cases
   of Elementary Teachers' Classroom Talk},
Journal = {INTERNATIONAL JOURNAL OF SCIENCE AND MATHEMATICS EDUCATION},
Year = {2022},
Volume = {20},
Number = {8},
Pages = {1927-1948},
Month = {DEC},
Abstract = {There has been an ongoing discussion about the role and level of teacher
   scaffolding in developing student agency while also meeting intended
   learning outcomes in inquiry-based science classrooms. This study
   investigates how teachers practice classroom talk to scaffold students'
   learning through the inquiry process and how teacher authority and
   student agency are embedded in a dialogical practice. A case study using
   an ethnographic approach was employed to understand the complexity of
   teachers' and students' interactions through classroom talk. Two
   elementary teachers and their students were involved in this study.
   Through classroom talk, the teachers demonstrated openness and
   flexibility in their dialogical turns to develop students' agency in
   problem solving, while at the same time employed their authority to
   monitor and lead classroom talk toward students' understanding of
   science concepts as part of the intended curriculum. They sought to
   emphasize scientific attitudes in the inquiry process by acknowledging
   the authority of science and scientists' ways of problem solving.
   Accordingly, this study found that the teachers alternated between
   dialogical and authoritative voices in classroom talk to facilitate
   student learning with agency. Furthermore, this study suggests the
   importance of distinguishing teacher pedagogical authority from teacher
   authority as power dominance.},
