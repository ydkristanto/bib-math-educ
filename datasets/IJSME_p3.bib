@article{ WOS:000312326700001,
Author = {Doorman, Michiel and Drijvers, Paul and Gravemeijer, Koeno and Boon,
   Peter and Reed, Helen},
Title = {TOOL USE AND THE DEVELOPMENT OF THE FUNCTION CONCEPT: FROM REPEATED
   CALCULATIONS TO FUNCTIONAL THINKING},
Journal = {INTERNATIONAL JOURNAL OF SCIENCE AND MATHEMATICS EDUCATION},
Year = {2012},
Volume = {10},
Number = {6},
Pages = {1243-1267},
Month = {DEC},
Abstract = {The concept of function is a central but difficult topic in secondary
   school mathematics curricula, which encompasses a transition from an
   operational to a structural view. The question in this paper is how the
   use of computer tools may foster this transition. With domain-specific
   pedagogical knowledge on the learning of function as a point of
   departure and the notions of emergent modeling and instrumentation as
   design heuristics, a potentially rich technology-intensive learning
   arrangement for grade 8 students was designed and field-tested. The
   results suggest that the relationship between tool use and conceptual
   development benefits from preliminary activities, from tools offering
   representations that allow for progressively increasing levels of
   reasoning, and from intertwinement with paper-and-pencil work.},
