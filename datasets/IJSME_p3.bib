@article{ WOS:000337042500009,
Author = {Norton, Anderson and Deater-Deckard, Kirby},
Title = {MATHEMATICS IN MIND, BRAIN, AND EDUCATION: A NEO-PIAGETIAN APPROACH},
Journal = {INTERNATIONAL JOURNAL OF SCIENCE AND MATHEMATICS EDUCATION},
Year = {2014},
Volume = {12},
Number = {3},
Pages = {647-667},
Month = {JUN},
Abstract = {Because of their focus on psychological structures and operations,
   neo-Piagetian approaches to learning lend themselves to neurological
   hypotheses. Recent advances in neural imaging and educational technology
   now make it possible to test some of these claims. Here, we take a
   neo-Piagetian approach to mathematical learning in order to frame two
   studies involving the use of electroencephalography and functional
   magnetic resonance imaging imaging, as well as the use of iOS-based apps
   designed to elicit particular ways of operating with mathematics.
   Results could inform theories of mathematical learning and effective
   educational game design.},
