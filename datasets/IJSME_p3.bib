@article{ WOS:000373180600004,
Author = {Tas, Yasemin and Sungur, Semra and Oztekin, Ceren},
Title = {DEVELOPMENT AND VALIDATION OF SCIENCE HOMEWORK SCALE FOR MIDDLE-SCHOOL
   STUDENTS},
Journal = {INTERNATIONAL JOURNAL OF SCIENCE AND MATHEMATICS EDUCATION},
