@article{ WOS:000331957200005,
Author = {Alegria, Adelina},
Title = {SUPPORTING ENGLISH LANGUAGE LEARNERS IN THE SCIENCE CLASSROOM THROUGH
   CRITICAL PEDAGOGY},
