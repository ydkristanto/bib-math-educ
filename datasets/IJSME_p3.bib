@article{ WOS:000540951200001,
Author = {Bingolbali, Erhan and Demir, Gokhan and Monaghan, John D.},
Title = {Knowledge of Sets: a Didactic Phenomenon},
Journal = {INTERNATIONAL JOURNAL OF SCIENCE AND MATHEMATICS EDUCATION},
Year = {2021},
Volume = {19},
Number = {6},
Pages = {1187-1208},
Month = {AUG},
Abstract = {This paper examines a didactic phenomenon, a perception that the
   elements of sets have a common property, e.g. that \{1, 2, 3\} and \{a,
   b, c\} are sets but \{1, 2, 3, a, b, c\} is not a set. The paper reports
   manifestations of this phenomenon in one country: across a range of
   school students in textbooks and in curricula and across pre-service and
   in-service teachers. The paper considers interpretations of the results
   under various theoretical frameworks and argues that these
   interpretations can be synthesized around the anthropological theory of
   the didactic. The paper ends with implications for what is taught under
   the name `mathematics'.},
