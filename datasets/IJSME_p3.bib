@article{ WOS:000535393200001,
Author = {Sabbah, Soryna and Heyd-Metzuyanim, Einat},
Title = {Integration of Arab Female Students at a Technological
   University-Narratives of Identity in Figured Worlds},
Journal = {INTERNATIONAL JOURNAL OF SCIENCE AND MATHEMATICS EDUCATION},
