@article{ WOS:000725413500001,
Author = {Qi, Chunxia and Cao, Chen and Huang, Rongjin},
Title = {Teacher Learning Through Collaboration Between Teachers and Researchers:
   a Case Study in China},
Journal = {INTERNATIONAL JOURNAL OF SCIENCE AND MATHEMATICS EDUCATION},
