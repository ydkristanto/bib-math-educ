@article{ WOS:000871339100001,
Author = {Simon, Martin A.},
Title = {Understanding the nature of arithmetical concepts-important content for
   the education of primary mathematics teachers},
Journal = {MATHEMATICAL THINKING AND LEARNING},
Year = {2022},
Month = {2022 OCT 24},
Abstract = {For teachers to be effective in promoting understanding of mathematical
   concepts, they need to understand the nature of mathematical concepts.
   However, to date, the mathematics education literature has not focused
   on this important area of teacher understanding. One reason that
   attention to this issue has been lacking is that the term ``mathematical
   concept{''} has generally been used with little specification. In recent
   theoretical work, I have explicated two types of mathematical
   (arithmetical) concepts. The first type involves knowledge of the
   logical necessity of a mathematical relationship. The second type
   specifies what it means to understand an arithmetic operation. In this
   commentary, I examine the implications of this work for primary teacher
   understanding about the nature of arithmetical concepts and challenges
   inherent in promoting those understandings.},
