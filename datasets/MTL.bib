@article{ WOS:000373959000003,
Author = {Rathe, Sanne and Torbeyns, Joke and Hannula-Sormunen, Minna M. and
   Verschaffel, Lieven},
Title = {Kindergartners' Spontaneous Focusing on Numerosity in Relation to Their
   Number-Related Utterances During Numerical Picture Book Reading},
Journal = {MATHEMATICAL THINKING AND LEARNING},
Year = {2016},
Volume = {18},
Number = {2},
Pages = {125-141},
Month = {APR 2},
Abstract = {This study investigated the relationship between kindergartners'
   Spontaneous Focusing on Numerosity (SFON) and their number-related
   utterances during numerical picture book reading. Forty-eight 4- to
   5-year-olds were individually interviewed via a SFON Imitation Task and
   a numerical picture book reading activity. We expected differences in
   the frequency of number-related utterances during picture book reading
   between children with a higher SFON score, providing more number-related
   utterances, and children with a lower SFON score. Our results showed
   large inter-individual differences in both kindergartners' SFON and the
