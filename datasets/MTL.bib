@article{ WOS:000208013500003,
Author = {Cramer, Kathleen and Wyberg, Terry},
Title = {Efficacy of Different Concrete Models for Teaching the Part-Whole
   Construct for Fractions},
Journal = {MATHEMATICAL THINKING AND LEARNING},
Year = {2009},
Volume = {11},
Number = {4},
Pages = {226-257},
Abstract = {The effectiveness of different concrete and pictorial models on
   students' understanding of the part-whole construct for fractions was
   investigated. Using interview data from fourth and fifth grade students
   from three different districts that adopted the Mathematics Trailblazers
   series, authors identified strengths and limitations of models used.
