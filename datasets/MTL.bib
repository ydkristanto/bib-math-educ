@article{ WOS:000293191200002,
Author = {Lee, Soo Jin and Brown, Rachael Eriksen and Orrill, Chandra Hawley},
Title = {Mathematics Teachers' Reasoning About Fractions and Decimals Using Drawn
   Representations},
Journal = {MATHEMATICAL THINKING AND LEARNING},
Year = {2011},
Volume = {13},
Number = {3},
Pages = {198-220},
Abstract = {This qualitative study considers middle grades mathematics teachers'
   reasoning about drawn representations of fractions and decimals. We
   analyzed teachers' strategies based on their response to multiple-choice
   tasks that required analysis of drawn representations. We found that
   teachers' flexibility with referent units played a significant role in
   understanding drawn representations with fractions and decimals.
   Teachers who could correctly identify or flexibly use the referent unit
   could better adapt their mathematical knowledge of fractions validating
   their choice, whereas teachers who did not attend to the referent unit
   demonstrated four problem-solving strategies for making sense of the
