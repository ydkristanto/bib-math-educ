@article{ WOS:000293190600004,
Author = {Langrall, Cynthia and Nisbet, Steven and Mooney, Edward and Jansem,
   Sinchai},
Title = {The Role of Context Expertise When Comparing Data},
Journal = {MATHEMATICAL THINKING AND LEARNING},
Year = {2011},
Volume = {13},
Number = {1-2, SI},
Pages = {47-67},
Abstract = {Our research addresses the role that context expertise plays when
   students compare data. We report findings from a study conducted in 3
   countries: Australia, United States, and Thailand. In each country, six
   middle school students analyzed authentic data relating to selected
   students' areas of interest. We examined the data analysis processes and
   discussion among students as each country cohort worked in two groups of
   three, where only one group included a student with particular expertise
   with the data context. We found that students used context knowledge to
   (a) bring new insight or additional information to the task, (b) explain
   the data, (c) provide justification or qualification for claims, (d)
   identify useful data for the task at hand, and (e) state facts that may
   enhance the picture of the data but are irrelevant to the process of
   analyzing the data. Implications for practice are discussed.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
