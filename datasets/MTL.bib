@article{ WOS:000293189800002,
Author = {Nickerson, Susan D. and Whitacre, Ian},
Title = {A Local Instruction Theory for the Development of Number Sense},
Journal = {MATHEMATICAL THINKING AND LEARNING},
Year = {2010},
Volume = {12},
Number = {3},
Pages = {227-252},
Abstract = {Gravemeijer's (1999, 2004) construct of a local instruction theory
   suggests a means of offering teachers a framework of reference for
   designing and engaging students in a set of sequenced, exemplary
   instructional activities that support students' mathematical development
   for a focused concept. In this paper we offer a local instruction theory
   to guide the design of a set of instructional activities in support of
   the development of number sense. We make explicit the goals,
   assumptions, underlying rationale, and related instructional activities
   and provide examples from a mathematics content course for preservice
   elementary teachers. In this way, we contribute to an elaboration of the
   construct of local instruction theory.},
