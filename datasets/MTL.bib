@article{ WOS:000293190300001,
Author = {Zazkis, Rina and Leikin, Roza},
Title = {Advanced Mathematical Knowledge in Teaching Practice: Perceptions of
   Secondary Mathematics Teachers},
Journal = {MATHEMATICAL THINKING AND LEARNING},
Year = {2010},
Volume = {12},
Number = {4},
Pages = {263-281},
Abstract = {For the purpose of our research we define Advanced Mathematical
   Knowledge (AMK) as knowledge of the subject matter acquired during
   undergraduate studies at colleges or universities. We examine the
   responses of secondary school teachers about their usage of AMK in
   teaching. We find that the majority of teachers focus on the purposes
   and advantages of their AMK for student learning, such as personal
   confidence, the ability to make connections, and to respond to students'
   questions; only a few provide content-specific examples. We conclude
   with a call for a more articulated relationship between AMK and
   mathematical knowledge for teaching.},
