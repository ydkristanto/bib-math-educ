@article{ WOS:000343278000005,
Author = {Alvarado, Hugo and Galindo, Maritza and Retamal, Lidia},
Title = {UNDERSTANDING OF SAMPLING DISTRIBUTION THROUGH DIDACTIC CONFIGURATIONS
   AND ITS IMPLICATION IN STATISTICAL INFERENCE},
Journal = {ENSENANZA DE LAS CIENCIAS},
Year = {2013},
Volume = {31},
Number = {2},
Pages = {75-91},
Month = {JUN},
Abstract = {We present a study on the understanding of sampling distributions and
   their connection with parameter estimation by confidence intervals. The
   teaching of statistical concepts, aimed at university students, is based
   on the implementation of settings configurations. Was applied two open
   problems and a questionnaire on confidence intervals Olivo, Batanero and
   Diaz (2008) to a group of 113 students. The results indicate
   improvements in the understanding of the estimated parameters, the
   coefficients relating to trust and choosing the appropriate sampling
   distribution to construct confidence intervals. In addition, the
   findings algebraic procedural difficulties of sampling distributions.},
