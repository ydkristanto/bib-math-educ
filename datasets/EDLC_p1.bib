@article{ WOS:000328095300006,
Author = {Garcia-Carmona, Antonio and Criado, Ana M.},
Title = {TEACHING THE TOPIC OF ENERGY TO 6-12 YEAR OLDS: AN APPROACH USING
   MACHINES FROM THE SCIENCE CURRICULUM},
Journal = {ENSENANZA DE LAS CIENCIAS},
Year = {2013},
Volume = {31},
Number = {3},
Pages = {87-102},
Month = {NOV},
Abstract = {This paper proposes a method for teaching energy in primary school based
   on machines from the science curriculum. A literature review on the
   different approaches to teaching energy and students' associated
   learning difficulties are presented. A hypothesis is presented
   consisting of three levels for progressive introduction of the concepts.
   This is followed by a discussion on the application for the development
   of students' scientific competencies. Emphasis is placed on the
   suggested learning outcomes for each education level. Lastly, a didactic
   classroom activity is presented in the context of inquiry-based learning
   to develop students conceptual understandings related to energy and
   machines.},
