@article{ WOS:000328095300008,
Author = {Pecharroman, Cristina},
Title = {MATHEMATICAL OBJECTS NATURE: REPRESENTATION AND MEANING},
Journal = {ENSENANZA DE LAS CIENCIAS},
Year = {2013},
Volume = {31},
Number = {3},
Pages = {121-134},
Month = {NOV},
Abstract = {This article describes the nature of mathematical objects and its reason
   for existence from a cognitive approach. It is considered that the
   reason of existence of mathematical objects is to be or represent a
   functionality in one context. And it is associated the nature of
   mathematical objects with their origin. That is, mathematical objects
   are or represent an organizational or interpretive functionality in the
   context, a functionality that becomes object. The representation of the
   mathematical object appears as an expression of the functionality it
   represents. Also, the institutional meaning of a mathematical object is
   developed from its functionality. Finally, it is interpreted the
   learning of mathematical objects from the description made of them in
   this investigation.},
