@article{ WOS:000766629700006,
Author = {Ma Marcos-Merino, Jose and Esteban Gallego, Rocio and Ochoa de Alda,
   Jesus A. G.},
Title = {The interplay of prior knowledge, emotions and learning in a science
   experiment activity},
Journal = {ENSENANZA DE LAS CIENCIAS},
Year = {2022},
Volume = {40},
Number = {1},
Pages = {107-124},
Abstract = {center dot Future Primary teachers describe negative emotions toward
   science. According to psychological and neuroscientific theories about
   academic performance and emotion-cognition integration, these emotions
   could be linked to their past and future learning. To ascertain this, we
   analyse a sample of 419 Primary pre-service teachers for the
   associations among their anticipated emotions (before an experimental
   activity of microbiology), their prior knowledge and their subsequent
   learning after the activity. Results indicate that insufficient prior
   knowledge and a non-scientific itinerary during Upper Secondary
   Education (i. e., social sciences or arts itineraries) characterize
   students who anticipate more negative emotions. These emotions modulate
   subsequent learning with a bivalent effect: frustration (a depressant
   emotion) repress it, whereas nervousness (stimulating emotion) favours
   it.},
Publisher = {UNIV AUTONOMA BARCELONA},
Address = {VICERECTORAT RELACIONS INSTITUCIONALS \& CULTURA, UNIV VALENCIA,
