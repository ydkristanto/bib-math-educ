@article{ WOS:000378148600009,
Author = {Arce Sanchez, Matias and Conejo Garrote, Laura and Ortega del Rincon,
   Tomas},
Title = {How are the mathematics notes written by a student? Influence of the
   mathematical elements and their relations},
Journal = {ENSENANZA DE LAS CIENCIAS},
Year = {2016},
Volume = {34},
Number = {1},
Pages = {149-172},
Abstract = {This research has been developed in three classrooms of high school
   students (16-17 years old) with a traditional methodology. We have
   compared the different kinds of elements and relations presented by the
   teachers with the notes written by the students in their notebooks. The
   topic is derivative rules and techniques. The framework is the content
   analysis, using maps of elements and relations in order to do this
   comparison. We have found a wide range of transcription profiles in
   students, especially in contexts where the teacher combines the
   obtaining of rules through its justifications with their practical
   application. The notes with a procedural and symbolic approach are the
   majority, influenced by the blackboard. There is a low transcription of
   the relational nature elements that are presented by the teachers in
   their oral discourse.},
