@article{ WOS:000587441700008,
Author = {Valverde-Crespo, Daniel and de Pro-Bueno, Antonio and Gonzalez-Sanchez,
   Joaquin},
Title = {Secondary students' digital competence when searching and selecting
   scientific information},
Journal = {ENSENANZA DE LAS CIENCIAS},
Year = {2020},
Volume = {38},
Number = {3},
Pages = {81-103},
Abstract = {Nowadays, Internet is the first source of information on science and
   technology, and the development of competences for using this socially
   established tool is considered as desirable for the scientific literacy
   of citizens. For this reason, this study intends to describe the digital
   competences which are shown among students in the third year of
   Compulsory Secondary Education (ESO in Spanish), to search, select and
   use information on a scientific topic, more particularly, on acid rain.
   After applying a questionnaire, two different analysis techniques are
   used to assess the participants' responses. We identify different
   sequences to search in network, to use the information and to assess it.},
