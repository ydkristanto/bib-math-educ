@article{ WOS:000290553000006,
Author = {Jaen Garcia, Mercedes and Palop Navarro, E.},
Title = {What do the pupils and teachers of a Secondary Education center think of
   and say about water, energy and waste management?},
Journal = {ENSENANZA DE LAS CIENCIAS},
Year = {2011},
Volume = {29},
Number = {1},
Pages = {61-74},
Month = {MAR},
Abstract = {In the context of an audit on the environmental management of the water,
   energy and the production of residues, in a center of Secondary
   Education, a study has been realized on the knowledge, attitudes,
   beliefs and behaviors of the pupils. From the results obtained a
   progressive improvement of his knowledge and attitudes depending on the
   educational skilled level has not been deduced. The students declare
   themselves very aroused but his actions are not coherent with an active
   participation in the solution of the problems. We have also investigated
   on the conducts of the teachers and the educational strategies that they
   propose to manage to modify the attitudes and behaviors of the students.},
