@article{ WOS:000460309000004,
Author = {Rodriguez Jara, Miguel Alejandro and Mena Lorca, Arturo and Mena Lorca,
   Jaime Juan Fernando and Vasquez Saldias, Patricia and Del Valle Leo,
   Maria},
Title = {Cognitive construction of the solution set of a system of linear
   equations with two unknowns},
Journal = {ENSENANZA DE LAS CIENCIAS},
Year = {2019},
Volume = {37},
Number = {1},
Pages = {71-92},
Abstract = {In this research, we propose a genetic decomposition for the solution
   set of a system of linear equations with two unknowns, by means of a
   transit from a homogeneous to a non-homogeneous linear system, in a
   Cartesian geometric context. To validate our genetic decomposition, we
   designed instruments that we applied to students from a secondary school
   mathematics teacher training program. Thus, and by using implicative
   statistics, we were able to confirm the mental constructions and
   mechanisms considered in our genetic decomposition. The results show
   lack of understanding of what a solution for a system is, difficulties
   in articulating the geometrical and algebraic aspects, and the
   convenience of using an alternative strategy in the case of systems of
   three or more linear equations.},
