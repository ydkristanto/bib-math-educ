@article{ WOS:000318824900010,
Author = {Hugo, Diana and Sanmarti, Neus and Aduriz-Bravo, Agustin},
Title = {STYLES OF EMOTIONAL WORK OF PROSPECTIVE SCIENCE TEACHERS DURING THEIR
   TEACHING PRACTICE},
Journal = {ENSENANZA DE LAS CIENCIAS},
Year = {2013},
Volume = {31},
Number = {1},
Pages = {153-170},
Month = {MAR},
Abstract = {This paper presents an emerging line of work on prospective science
   teachers' emotions while teaching a unit during their teaching practice.
   It aims to help improve the current educational theories that form
   models for pre- and in-service training of science teachers. We have
   found in the six cases investigated four styles of emotional work:
   sensible, seductive, overwhelmed and phlegmatic. These have been
   characterized by different variables that appear to influence change or
   permanence in the initial teaching model, such as the secondary emotion
   generated and its management when metacognitively reflecting on the
   difficulties encountered in teaching.},
