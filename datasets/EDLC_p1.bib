@article{ WOS:000368649700004,
Author = {Berciano, Ainhoa and Ortega del Rincon, Tomas and Puerta Rebuel,
   Milagros},
Title = {Learning of graphical and algebraic interpolations. Comparative analysis},
Journal = {ENSENANZA DE LAS CIENCIAS},
Year = {2015},
Volume = {33},
Number = {3},
Pages = {43-58},
Abstract = {In this paper we present an empirical study on the Graphical and
   Algebraic interpolation/extrapolation with students of High School of
   Social Sciences. We prove that, if the problem of the
   interpolation/extrapolation of a function is addressed with specific
   instructions and the appropriate graphical templates, the graphical
   interpolation and extrapolation are easier for students than the
   algebraic, obtaining a positive meaningful difference in the academic
   results with the graphic method. Furthermore, when students
   interpolate/extrapolate algebraically, the difficulty of the resolution
   depends on function type and the number and type of parameters, while in
   the case of graphical interpolation/extrapolation this dependency
   doesn't happen.},
