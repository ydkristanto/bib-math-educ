@article{ WOS:000414258100008,
Author = {Cuevas Vallejo, Carlos Armando and Villamizar Araque, Freddy Yesid and
   Martinez Uribe, Alfredo},
Title = {Educational activities for the pitch, as a quality of sound, in basic
   level physics courses, mediated by digital technology},
Journal = {ENSENANZA DE LAS CIENCIAS},
Year = {2017},
Volume = {35},
Number = {3},
Pages = {129-150},
Abstract = {The purpose of this paper is to encourage pitch understanding as quality
   of sound and mathematical concepts associated to its modelling, through
   the design and application of didactic activities. Sound as a concept
   and its qualities are part of the studies of second degree physics basic
   education program in Mexico, and fourth degree of secondary in Colombia
   (ages between 13 and 15 years). For the design of the activities it was
   necessary to use computer applications (apps) arranged in mobile devices
   as a portable laboratory, a didactic framework and the development of a
   modelling proposal. The experience was implemented in two countries and
   was developed in 4 phases. Evidences of conceptual change in students
   will be shown.},
