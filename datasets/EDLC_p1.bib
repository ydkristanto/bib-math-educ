@article{ WOS:000314440000012,
Author = {Garcia, Mercedes and Gavilan, Jose-Maria and Llinares, Salvador},
Title = {A SECONDARY MATHEMATICS TEACHER'S PRACTICE PERSPECTIVE ON THE DERIVATE
   TEACHING. RELATIONS BETWEEN TEACHER'S PERSPECTIVE AND PRACTICE},
Journal = {ENSENANZA DE LAS CIENCIAS},
Year = {2012},
Volume = {30},
Number = {3},
Pages = {219-235},
Month = {NOV},
Abstract = {This study aims to describe and explain teacher's practice. The teaching
   context was the introduction of derivative concept in secondary high
   school (16-18 year-old students). We introduce and use the notion
   ``modeling a mechanism of construction of knowledge{''} to explain how
   the teacher generated learning opportunities to his pupils derived from
   his perspective. The relation between teacher's perspective and his/her
   practice is describe by how mathematical content was organized, how were
   modeled the construction mechanisms of knowledge by the management of
   different representations systems and mathematical discourse in the
   classroom and it is explained by the complementarity between cognitive
   and sociocultural references.},
