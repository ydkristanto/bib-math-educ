@article{ WOS:000334264700007,
Author = {Camacho Moreno, Vladimir and Sanchez Pozos, Jose Javier and Zubieta
   Badillo, Gonzalo},
Title = {Can science's students recognize the logical arguments involved in a
   demonstration?},
Journal = {ENSENANZA DE LAS CIENCIAS},
Year = {2014},
Volume = {32},
Number = {1},
Pages = {117-138},
Month = {MAR},
Abstract = {In every mathematical proof logically correct arguments are used; it is
   a necessary condition to find out them, and also to recognize how they
   are applied, for understanding some of the key steps in the
   demonstrations. But can a student clearly identify the arguments used in
   a demonstration? And if he recognizes them, is he able to explain how
   they were used? The aim of this paper is to explore if mathematics' and
   computer science's students who have met demonstrations at their
   university, master or even doctoral courses can successfully identify
   and explain the arguments within a demonstration.},
