@article{ WOS:000345604600010,
Author = {Quilez-Diaz, Ana and Quilez-Pardo, Juan},
Title = {Pre-university and university general Chemistry textbooks' definition
   and calculation of equilibrium constants},
Journal = {ENSENANZA DE LAS CIENCIAS},
Year = {2014},
Volume = {32},
Number = {3, 1},
Pages = {187-203},
Month = {NOV},
Abstract = {In this paper, after presenting the experimental equilibrium constants,
   K and Ke and the thermodynamic equilibrium constant, K., it is analysed
   the misrepresentation of them by general chemistry textbooks. We examine
   the way both first-year university chemistry textbooks and preuniversity
   chemistry textbooks handle the units of equilibrium constants. Many
   textbooks do not report the corresponding units in the calculation of Kp
   and K. Moreover, in most of the cases, K. is not defined and as a result
   confusion about these concepts is widespread. This problem in the
   terminology used by textbooks causes that many of them confuse Kp with
   K.. It is suggested that preuniversity textbooks should correctly define
   both Kp and K, leaving the definition of the thermodynamic constant, Ko,
   as well as its relationship to K and K, to first-year university
   textbooks.},
