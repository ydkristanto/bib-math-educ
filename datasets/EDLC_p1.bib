@article{ WOS:000384933300006,
Author = {Claros Mellado, Javier and Coriat Benarroch, Moises and Sanchez Compana,
   Teresa},
Title = {Cognitive structure and phenomenology: the case of the convergent
   sequence},
Journal = {ENSENANZA DE LAS CIENCIAS},
Year = {2016},
Volume = {34},
Number = {2},
Pages = {87-105},
Abstract = {This research describes how two ``cells{''} of the cognitive structure
   (Vinner, 1991) connect to the phenomena, in Freudenthal's (1983) sense,
   organized by a definition of finite limit of a sequence and the
   phenomena organized by a Cauchy sequence (Claros, 2010). These
   connections arose when we faced, as a target, to the introduction of
