@article{ WOS:000388208700010,
Author = {Escudero, Consuelo and Jaime, Eduardo A. and Gonzalez, Sonia B.},
Title = {Towards quantum consciousness from the photoelectric effect},
Journal = {ENSENANZA DE LAS CIENCIAS},
Year = {2016},
Volume = {34},
Number = {3},
Pages = {183-200},
Abstract = {This article inquires about the resources that students employ when they
   answer to questions related to non tangible phenomena that relate the
   world in different scales. Such is the case of the electromagnetic
   radiation and matter interaction. This research was focused on three
   problem situations related to the photoelectric effect and five
   categories were identified within their answers making us think about
   the construction of a main concept of the natural sciences such as
   energy and its relationship with the ideas of radiation-matter
   interaction, wave-particle duality, quantum, among others. It is
   evidenced a net of subjectivities is generated so that it works as the
   support for the objective concept formation that is the ultimate goal of
   the process of teaching.},
