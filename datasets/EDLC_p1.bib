@article{ WOS:000345604900001,
Author = {Carrasco Henriquez, Eduardo Andres and Diaz Moreno, Leonora and Buendia
   Abalos, Gabriela},
Title = {Figuration of variation},
Journal = {ENSENANZA DE LAS CIENCIAS},
Year = {2014},
Volume = {32},
Number = {3, 2},
Pages = {365-384},
Month = {NOV},
Abstract = {This article presents a research about practices of construction and
   interpretation of a figure representing ostensive and non-ostensive
   entities in a variation phenomenon. It is a qualitative case study,
   investigating some practices of figuration based on the Cartesian's
   graph historical constitution and other cases on student's productions.
   It is an interpretive study, since it seeks to understand figurative
   practices cases of scientists and other cases of student's figuration.
   It is analytical, since the study seeks analytical factors in figuration
   practices embedded in these practices. And relational, since it seeks to
   establish links and dissonance, regarding the elements that are being
   configured. We understand the student and historical figurations of what
   varies from an epistemology of figuration perspective.},
