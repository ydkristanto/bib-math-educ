@article{ WOS:000343278000010,
Author = {Maria Lopez-Valentin, Dulce and Teresa Guerra-Ramos, Maria},
Title = {ANALYSIS OF THE LEARNING ACTIVITIES INCLUDED IN NATURAL SCIENCES
   TEXTBOOKS FOR PRIMARY EDUCATION IN MEXICO},
Journal = {ENSENANZA DE LAS CIENCIAS},
Year = {2013},
Volume = {31},
Number = {2},
Pages = {173-191},
Month = {JUN},
Abstract = {This paper presents an analysis of the learning activities included in
   two textbooks for primary sciences, distributed in Mexico by the
   Ministry of Education A qualitative and quantitative evaluation was made
   of the objectives of learning, procedural contents, and potential for
   promoting learning. The results indicated that the objectives of
   obtaining knowledge and applying theory prevail in both books, in terms
   of the procedural content; the tendency was to promote observation,
   search of information and communication mainly. Regarding the learning
   promotion potential, only one-half of the activities had an acceptable
   level, according to the logic of our analysis.},
