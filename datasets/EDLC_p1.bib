@article{ WOS:000414258100002,
Author = {de Pro Chereguini, Carlos and de Pro Bueno, Antonio and Serrano Pastor,
   Francisca},
Title = {Do teachers in training know which subcompetences are they working on
   when designing teaching activities?},
Journal = {ENSENANZA DE LAS CIENCIAS},
Year = {2017},
Volume = {35},
Number = {3},
Pages = {7-28},
Abstract = {Often our initial training programs try to incorporate the ``latest{''}
   knowledge of experimental science teaching (DCE), but it is not always
   published what effects it has on the learning of future teachers. In one
   of our subjects, we had worked on contents related to the basic
   competences: what are they, in what subcompetences are they broken down,
