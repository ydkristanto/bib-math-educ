@article{ WOS:000543378300005,
Author = {Montero, Eloisa and Luz Callejo, Maria and Valls, Julia},
Title = {Instrumentation of a progression of strategies by prospective primary
   teachers},
Journal = {ENSENANZA DE LAS CIENCIAS},
Year = {2020},
Volume = {38},
Number = {2},
Pages = {83-101},
Abstract = {The aim of this research is to characterize how prospective primary
   teachers (PPT) make use of a progression of multiple-division
   problem-solving strategies as a conceptual instrument to interpret
   children's mathematical thinking. 61 PPT participated in a teaching
   experiment whose purpose was to develop the teaching competence
   ``professional noticing{''} primary students' mathematical thinking. The
   use of the progression of strategies by the PPT was analyzed by means of
   an instrumental approach to characterize how they identified and
   interpreted strategies that were used by children. The results show
   three different groups of PPT, according to the use of the progression
   of strategies: those who did not use the progression, considering it an
   artifact; those who used certain characteristics of the progression, and
   generated instrumental action schemes, and those who used all the
   information and instrumented the progression.},
