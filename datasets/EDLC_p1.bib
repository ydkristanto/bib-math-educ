@article{ WOS:000510440600008,
Author = {Bustos Rubilar, Alvaro Sebastian and Zubieta Badillo, Gonzalo},
Title = {Development and changes in the students' ways of mathematical
   justification when working in a sociocultural environment},
Journal = {ENSENANZA DE LAS CIENCIAS},
Year = {2019},
Volume = {37},
Number = {3},
Pages = {129-148},
Abstract = {In this article we report how an approach was promoted from the
   pre-formal to the formal stage regarding the students' way of justifying
   mathematical reasoning. We implemented an activity based on the ACODESA
   teaching method to create a sociocultural environment in the mathematics
   class and to promote social interactions between the students. We
   present the case of a learner who changed his way of justifying after
   exchanging ideas and discussing with peers during the analysis of the
   justifications generated by each of them.},
