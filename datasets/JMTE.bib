@article{ WOS:000716842900002,
Author = {Lee, Mi Yeon and Lee, Ji-Eun},
Title = {An analysis of elementary prospective teachers' noticing of student
   pattern generalization strategies in mathematics},
Journal = {JOURNAL OF MATHEMATICS TEACHER EDUCATION},
Year = {2023},
Volume = {26},
Number = {2},
Pages = {155-177},
Month = {APR},
Abstract = {In this study, hypothetical samples of students' work on a task
   involving pattern generalizations were used to examine the
   characteristics of the ways in which 154 elementary prospective teachers
   (PSTs) paid attention to students' work in mathematics. The analysis
   included what the PSTs attended to, their interpretations, and their
   suggestions for supporting students' further learning. While the
   findings showed that the PSTs noticed a wide range of aspects of
   mathematical learning, their analyses of students' thinking and their
   ideas for supporting students' learning lacked sophistication. Also,
   there was considerable divergence between the students' thinking as
   evidenced in the hypothetical problem solutions and the PSTs' suggested
   support. Based on these findings, we provide some suggestions for
   teacher education.},
