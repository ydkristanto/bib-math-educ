@article{ WOS:000739786200001,
Author = {Ding, Meixia and Li, Xiaobao and Manfredonia, Monica L. and Luo, Wenda},
Title = {US and Chinese elementary teachers' noticing of cross-cultural
   mathematics videos},
Journal = {JOURNAL OF MATHEMATICS TEACHER EDUCATION},
Year = {2023},
Volume = {26},
Number = {2},
Pages = {211-239},
Month = {APR},
Abstract = {This study aims to explore teacher noticing differences of the sampled
   US and Chinese elementary teachers from cross-cultural mathematics
   videos. A total of 34 expert teachers commented on 25 video clips
   online. We coded what and how teachers noticed from the videos both
   quantitatively and qualitatively. Findings reveal teachers' strong
   interests and profound reflections, especially in the teaching domain
   including representations, communication, and teacher questioning/guide.
   Cross-cultural differences in teacher noticing were identified, which
   were discussed based on possible cultural influences. Implications in
   research and practice, teacher support, and methodology are discussed.},
