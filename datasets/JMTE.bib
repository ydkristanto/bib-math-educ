Author = {Kutaka, T. S. and Ren, L. and Smith, W. M. and Beattie, H. L. and
   Edwards, C. P. and Green, J. L. and Chernyavskiy, P. and Stroup, W. and
   Heaton, R. M. and Lewis, W. J.},
Title = {Examining change in K-3 teachers' mathematical knowledge, attitudes, and
   beliefs: the case of Primarily Math},
Journal = {JOURNAL OF MATHEMATICS TEACHER EDUCATION},
Year = {2018},
Volume = {21},
Number = {2},
Pages = {147-177},
Month = {APR},
Abstract = {This study examines the impact of the Primarily Math Elementary
   Mathematics Specialist program on K-3 teachers' mathematical content
   knowledge for teaching, attitudes toward learning mathematics, and
   beliefs about mathematics teaching and learning. Three cohorts of
   teachers participating in the program were compared to a similar group
   of non-participating teachers. Teacher outcomes were measured
   longitudinally across 5 years. Participating teachers showed changes in
   their knowledge, attitudes, and beliefs in line with program goals
   immediately after completion of coursework. Moreover, these changes were
   sustained in subsequent years, following program completion. Relative to
   the comparison group, participants demonstrated greater gains in
   knowledge as well as greater improvements in attitudes and beliefs.
   Implications of these results for professional development design,
   implementation, and evaluation are discussed.},
