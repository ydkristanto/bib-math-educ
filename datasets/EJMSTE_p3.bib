@article{ WOS:000382668700006,
Author = {Kim, Pyoung Won},
Title = {The Wheel Model of STEAM Education Based on Traditional Korean
   Scientific Contents},
Journal = {EURASIA JOURNAL OF MATHEMATICS SCIENCE AND TECHNOLOGY EDUCATION},
