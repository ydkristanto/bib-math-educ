@article{ WOS:000341620700006,
Author = {Swart, Arthur James},
Title = {Using Problem-Based Learning to Stimulate Entrepreneurial Awareness
   Among Senior African Undergraduate Students},
Journal = {EURASIA JOURNAL OF MATHEMATICS SCIENCE AND TECHNOLOGY EDUCATION},
Year = {2014},
Volume = {10},
Number = {2},
Pages = {125-134},
Month = {APR},
Abstract = {Literature states that entrepreneurial awareness is required in
   countries where entrepreneurship is neither publicized nor acclaimed,
   which is currently the situation in South Africa. Entrepreneurial skills
   include the ability to market one's product by means of a sales poster
   while problem-based learning is viewed as fundamental to entrepreneurial
   education. The research question arises ``How may academics use
   problem-based learning to stimulate entrepreneurial awareness among
   senior African undergraduate students?{''} The purpose of this article
   is to present a practical assignment which was designed to help students
