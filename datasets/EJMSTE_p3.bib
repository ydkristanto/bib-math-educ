@article{ WOS:000404591100003,
Author = {Chang, Yi-Hsing and Lin, Yu-Kai and Fang, Rong-Jyue and Lu, You-Te},
Title = {A Situated Cultural Festival Learning System Based on Motion Sensing},
Journal = {EURASIA JOURNAL OF MATHEMATICS SCIENCE AND TECHNOLOGY EDUCATION},
