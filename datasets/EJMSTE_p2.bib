@article{ WOS:000374307400014,
Author = {Devecioglu-Kaymakci, Yasemin},
Title = {Embedding Analogical Reasoning into 5E Learning Model: A Study of the
   Solar System},
Journal = {EURASIA JOURNAL OF MATHEMATICS SCIENCE AND TECHNOLOGY EDUCATION},
