@article{ WOS:000417628000057,
Author = {Gao, Yan and Zhu, Jianxin and Zhu, Jiawei},
Title = {The Influence of Competition Network Structure and Competitive Behavior
   on Customer Performance - An Empirical Study Based on Chinese Catering
   Industry},
Journal = {EURASIA JOURNAL OF MATHEMATICS SCIENCE AND TECHNOLOGY EDUCATION},
Year = {2017},
Volume = {13},
Number = {12},
Pages = {8193-8203},
Month = {DEC},
Abstract = {This study proposes and tests a theory of the effects of competition
   network structure and competitive behavior on customer performance.
   Competition networks are defined as the patterns of interdependence
   between rivals that emerge from direct competition. We propose that the
   size, density and heterogeneity of competition networks influence
   customer performance in a systemic way. The results show that network
   size and network density positively affect customer performance, and
   network heterogeneity has no effect on the customer performance. We also
   validate the partial mediation role of competitive behavior in the
   process of competitive network structures affect the customer
   performance. We test our hypotheses in a unique dataset drawn from the
   Chinese service industry in Beijing and Shanghai.},
