@article{ WOS:000409067500068,
Author = {Hu, Lei-Fang and Chen, Yong-Qiang},
Title = {Factors on Bilingual Teaching Effect on the Course of ``Introduction to
   E-commerce{''}},
Journal = {EURASIA JOURNAL OF MATHEMATICS SCIENCE AND TECHNOLOGY EDUCATION},
Year = {2017},
Volume = {13},
Number = {8},
Pages = {5273-5280},
Month = {AUG},
Abstract = {Under the background of education modernization, construction of
   bilingual teaching courses in colleges and universities is directly
   driven by project of undergraduate teaching quality and teaching reform
   launched by the Ministry of Education, and has been expanding rapidly.
   This paper, takes the course of ``introduction to e-commerce{''} as an
   object, measures the bilingual teaching effect from the four aspects,
   namely teachers' professional capacity, teachers' foreign language
   capacity, students' learning motivation and suitability of teaching
   materials. The results of the study show that, students learning
   motivation, teachers' professional capacity and teachers' foreign
   language capacity significantly impact the bilingual teaching effect of
   ``introduction to e-commerce{''}. Therefore, colleges and universities
   should actively guide students to interest in learning, stimulate
   students' learning motivation, improve teachers' foreign language level
   and enhance their professional quality, so as to improve the bilingual
   teaching effect of the course.},
