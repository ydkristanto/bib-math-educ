@article{ WOS:000404607800061,
Author = {Boh Podgornik, Bojana and Dolnicar, Danica and Glazar, Sasa Aleksej},
Title = {Does the Information Literacy of University Students Depend on their
   Scientific Literacy?},
Journal = {EURASIA JOURNAL OF MATHEMATICS SCIENCE AND TECHNOLOGY EDUCATION},
