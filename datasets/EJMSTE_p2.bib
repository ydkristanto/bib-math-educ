@article{ WOS:000423586100034,
Author = {Ertas, Sebnem and Acici, Funda Kurak and Bekar, Makbulenur},
Title = {Student Views on The Theme of ``Historically Narrated Urban Furniture
   Workshop{''} And Its Technological Uses in Design Education},
Journal = {EURASIA JOURNAL OF MATHEMATICS SCIENCE AND TECHNOLOGY EDUCATION},
Year = {2018},
Volume = {14},
Number = {1},
Pages = {413-426},
Month = {JAN},
Abstract = {Design is a scientific process that incorporates several different
   disciplines and responds to the requirements of the natural environment
   with aesthetics and creativity. Short term ``design studies{''} such as
   studio workshops, symposiums, workshops, student meetings, etc., where
   all disciplines including those based on design and creativity come
   together and various works are presented, are an important part of the
   education process. These include seminars, exhibitions, workshops and
   similar events that are held throughout the world, which are effective
   organizations in the design education process. In the present paper, the
   ``History Narrated with Urban Furniture, Konya / Sille{''} workshop that
   was organized within the context of BAP 01 project no. SBA-2016-5350 at
   Karadeniz Technical University in 2016-2017 academic year was discussed.
   The study was based on the data collected during the workshop activities
   conducted with 20 students. The study aimed to measure of the use of
   technology by the achievements of students in informal design education
   studies that become prevalent in Turkey and worldwide and their
   interaction with formal education based on the collected data. The
   survey results were based on the percentages (\%) with respect to total
   values. Finally, recommendations for future design training events were
   made based on the survey data.},
