@article{ WOS:000414587900062,
Author = {Li, Xuesong and Li, Hongyu and Huang, Chunlei and Yao, Qiang and Zhao,
   Xu and Chen, Xue and Hu, Qi},
Title = {Internet News Dissemination on Ideological and Moral Cultivation in
