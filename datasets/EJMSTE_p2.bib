@article{ WOS:000404607800064,
Author = {Yang, Il-Ho and Park, Sang-Woo and Shin, Jung-Yun and Lim, Sung-Man},
Title = {Exploring Korean Middle School Students' View about Scientific Inquiry},
Journal = {EURASIA JOURNAL OF MATHEMATICS SCIENCE AND TECHNOLOGY EDUCATION},
Year = {2017},
Volume = {13},
Number = {7},
Pages = {3935-3958},
Month = {JUL},
Abstract = {The aim of this study is to examine Korean middle school students' view
   about scientific inquiry with the Views about Scientific Inquiry (VASI)
   questionnaire, an instrument that deals with eight aspects of scientific
   inquiry. 282 Korean middle school students participated in this study,
   and their responses were classified as informed, mixed, and naive. The
   results revealed that Korean middle school students primarily held mixed
   or naive views about scientific inquiry. To discover why students held
   these views, their responses were analyzed in detail. For instance, they
   did not understand the meaning of `experiment', `scientific', `data',
   and `evidence' well. They understood scientific terminology and
   scientific inquiry in everyday contexts. Students also tended to regard
   science process skill as scientific method. According to the results
   mentioned above, explicit and reflective instruction is necessary to
   develop students' views about scientific inquiry.},
