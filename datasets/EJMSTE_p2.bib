@article{ WOS:000322077400002,
Author = {Kingir, Sevgi},
Title = {Using Non-traditional Writing as a Tool in Learning Chemistry},
Journal = {EURASIA JOURNAL OF MATHEMATICS SCIENCE AND TECHNOLOGY EDUCATION},
