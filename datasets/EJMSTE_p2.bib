@article{ WOS:000409067500009,
Author = {Hamada, Mohamed and Hassan, Mohamed},
Title = {An Enhanced Learning Style Index: Implementation and Integration into an
   Intelligent and Adaptive e-Learning System},
Journal = {EURASIA JOURNAL OF MATHEMATICS SCIENCE AND TECHNOLOGY EDUCATION},
