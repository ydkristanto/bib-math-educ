@article{ WOS:000322076100007,
Author = {Kabaca, Tolga},
Title = {Using Dynamic Mathematics Software to Teach One-Variable Inequalities by
   the View of Semiotic Registers},
Journal = {EURASIA JOURNAL OF MATHEMATICS SCIENCE AND TECHNOLOGY EDUCATION},
Year = {2013},
Volume = {9},
Number = {1},
Pages = {73-81},
Month = {FEB},
Abstract = {Solution set of any inequality or compound inequality, which has
   one-variable, lies in the real line which is one dimensional. So a
   difficulty appears when computer assisted graphical representation is
   intended to use for teaching these topics. Sketching a one-dimensional
   graph by using computer software is not a straightforward work. In this
   paper, an innovative approach is suggested by using GeoGebra, the free
   dynamic mathematics software. This approach was theoretically based on
   Sackur's suggestion, which is created by semiotic register theorem of
   Duval. The dynamic application, described in this paper, also provides
   an opportunity to observe more complex, even impossible to solve
   analytically, inequalities' solution sets visually. At the end of the
   paper, it is also explained that how this approach also has the
   potential of allowing the students to make detailed reasoning towards
   finding solutions of inequalities and compound inequalities.},
