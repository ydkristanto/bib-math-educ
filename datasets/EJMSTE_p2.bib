@article{ WOS:000393034500003,
Author = {Chen, Ru-Si},
Title = {Preschool Teachers' Attitudes toward Internet Applications for
   Professional Development in Taiwan},
Journal = {EURASIA JOURNAL OF MATHEMATICS SCIENCE AND TECHNOLOGY EDUCATION},
