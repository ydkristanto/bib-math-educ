@article{ WOS:000429001500020,
Author = {Gurkut, Cannur and Nat, Muesser},
Title = {Important Factors Affecting Student Information System Quality and
   Satisfaction},
Journal = {EURASIA JOURNAL OF MATHEMATICS SCIENCE AND TECHNOLOGY EDUCATION},
Year = {2018},
Volume = {14},
Number = {3},
Pages = {923-932},
Abstract = {Student Information System is one of the key systems for facilitating
   the management and development of Higher Education Institutions. Its use
   for academic decision-making purposes as well as other academic tasks is
   crucial. Therefore, this paper aims to understand the impact of System
   Quality, Information Quality and Information Presentation on Student
   Information System satisfaction of academic and administrative staff. In
   this study, System satisfaction survey is carried out and factor
   analysis and regression tests are applied to interpret the collected
   data. The results show that only Information Quality has direct effect
   on satisfaction. Then the impact of decision-making as a mediator factor
   on system satisfaction is measured and the results reveal that System
   Quality and Information Quality has indirect significant effect whereas
   Information Presentation does not have direct nor indirect effect on
   system satisfaction.},
