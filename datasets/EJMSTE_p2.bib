@article{ WOS:000417628000044,
Author = {Tang, Shujun},
Title = {Learning Mechanism and Function Characteristics of MOOC in the Process
   of Higher Education},
Journal = {EURASIA JOURNAL OF MATHEMATICS SCIENCE AND TECHNOLOGY EDUCATION},
