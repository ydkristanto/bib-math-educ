@article{ WOS:000414587900042,
Author = {Wang, Yongliang},
Title = {Construction Elements and Path of Practical Education Model in
   Universities},
Journal = {EURASIA JOURNAL OF MATHEMATICS SCIENCE AND TECHNOLOGY EDUCATION},
