@article{ WOS:000404607800028,
Author = {Liu, Hsin-Lung},
Title = {Planning Wetland Ecology-Based Outdoor Education Courses in Taiwanese
   Junior High Schools},
Journal = {EURASIA JOURNAL OF MATHEMATICS SCIENCE AND TECHNOLOGY EDUCATION},
