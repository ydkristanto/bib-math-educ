@article{ WOS:000382668700022,
Author = {Youchu, Huang},
Title = {A Qualitative Study on the Development of Pre-service Teachers'
   Mathematical Knowledge for Teaching in a History-based Course},
Journal = {EURASIA JOURNAL OF MATHEMATICS SCIENCE AND TECHNOLOGY EDUCATION},
Year = {2016},
