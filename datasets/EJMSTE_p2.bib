@article{ WOS:000404603400013,
Author = {Ho, Shyue-Yung and Chen, Wen-Te and Hsu, Wei-Ling},
Title = {Assessment System for Junior High Schools in Taiwan to Select
   Environmental Education Facilities and Sites},
Journal = {EURASIA JOURNAL OF MATHEMATICS SCIENCE AND TECHNOLOGY EDUCATION},
