Author = {Xiao, Peng-Wei and Fan, Kuo-Kuang and Xu, Song and Su, Chung-Ho},
Title = {A Preliminary Study on the Learning Satisfaction and Effectiveness of VR
   Weight Training Assisting Learning System for Beginners},
