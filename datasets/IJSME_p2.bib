@article{ WOS:000294538500007,
Author = {Chhokar, Kiran and Dua, Shweta and Taylor, Neil and Boyes, Edward and
   Stanisstreet, Martin},
Title = {INDIAN SECONDARY STUDENTS' VIEWS ABOUT GLOBAL WARMING: BELIEFS ABOUT THE
   USEFULNESS OF ACTIONS AND WILLINGNESS TO ACT},
Journal = {INTERNATIONAL JOURNAL OF SCIENCE AND MATHEMATICS EDUCATION},
Year = {2011},
Volume = {9},
Number = {5},
Pages = {1167-1188},
Month = {OCT},
Abstract = {A 44-item survey instrument was designed to determine secondary
   students' views about how useful various specific actions related to
   reducing global warming might be, their willingness to undertake these
   various actions and the extent to which these 2 might be linked. The
   instrument was administered to students in grades 6 - 10 (n = 768) from
   4 large schools in metropolitan Delhi, India in which the teaching
   language was English. The findings indicated that this cohort of Indian
   students exhibited high levels of concern about climate change and
   strong willingness to act against global warming and in favour of the
   environment. The findings are tentatively compared with those from 2
   similar survey studies conducted in Western contexts (Spain and
   Australia).},
