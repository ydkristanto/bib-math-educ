@article{ WOS:000852287900001,
Author = {Hsu, Hui-Yu and Yao, Chen-Yu and Lu, BingYang},
Title = {Examination of Taiwanese Mathematics Teacher Questioning},
Journal = {INTERNATIONAL JOURNAL OF SCIENCE AND MATHEMATICS EDUCATION},
Year = {2023},
Volume = {21},
