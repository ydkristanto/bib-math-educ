@article{ WOS:000325105700003,
Author = {Levenson, Esther and Gal, Hagar},
Title = {INSIGHTS FROM A TEACHER PROFESSIONAL DEVELOPMENT COURSE: RONA'S CHANGING
   PERSPECTIVES REGARDING MATHEMATICALLY-TALENTED STUDENTS},
Journal = {INTERNATIONAL JOURNAL OF SCIENCE AND MATHEMATICS EDUCATION},
Year = {2013},
Volume = {11},
Number = {5},
Pages = {1087-1114},
Month = {OCT},
Abstract = {This paper considers studying changes in teachers' perspectives
   regarding the teaching of mathematically talented students learning in
   heterogeneous classes. We investigate these changes through a case study
   of Rona, a teacher who participated in the professional development
   program ``From One End to the Other{''} held in The David Yellin College
   of Education, Israel. The analysis focuses on 3 categories of change:
   awareness, practice, and self-efficacy. The results concerning Rona, 1
   of the participants, demonstrate a rise in awareness of talented
   students, a change in the ways of meeting the needs of these students,
   and a rise in self-efficacy regarding the teaching of talented students.
   Moreover, it was found that changes in 1 category are interwoven with
   changes in the other categories.},
