Author = {Soneira, Carlos},
Title = {The Use of Representations when Solving Algebra Word Problems and the
   Sources of Solution Errors},
Journal = {INTERNATIONAL JOURNAL OF SCIENCE AND MATHEMATICS EDUCATION},
