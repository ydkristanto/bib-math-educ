@article{ WOS:000856603000001,
Author = {Sung, Euisuk and Kelley, Todd R.},
Title = {Elementary Students' Engineering Design Process: How Young Students
   Solve Engineering Problems},
Journal = {INTERNATIONAL JOURNAL OF SCIENCE AND MATHEMATICS EDUCATION},
Year = {2023},
Volume = {21},
