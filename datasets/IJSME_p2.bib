@article{ WOS:000292148000001,
Author = {Anderson, John O. and Chiu, Mei-Hung and Yore, Larry D.},
Title = {FIRST CYCLE OF PISA (2000-2006)-INTERNATIONAL PERSPECTIVES ON SUCCESSES
   AND CHALLENGES: RESEARCH AND POLICY DIRECTIONS},
Journal = {INTERNATIONAL JOURNAL OF SCIENCE AND MATHEMATICS EDUCATION},
