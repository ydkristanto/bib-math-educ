@article{ WOS:000391393300009,
Author = {Martinez-Sierra, Gustavo and del Socorro Garcia-Gonzalez, Maria},
Title = {Students' Emotions in the High School Mathematical Class: Appraisals in
   Terms of a Structure of Goals},
Journal = {INTERNATIONAL JOURNAL OF SCIENCE AND MATHEMATICS EDUCATION},
Year = {2017},
Volume = {15},
Number = {2},
Pages = {349-369},
Month = {FEB},
Abstract = {Little research in the field of Mathematics Education is directed
   towards emotions of students beyond their emotions in problem-solving.
   In particular, the daily emotions of students in a mathematics class
   have been sparsely studied in the field of mathematics education. In
   order to fill this gap, this qualitative research aims to identify high
   school students' emotional experiences in the mathematics classroom and
   identify the appraisal structures that support such emotional
   experiences. Focus group interviews were carried out until theoretical
   saturation of the data was achieved (N = 53 in nine focus groups
   interviews). Data analysis is based on the theory of cognitive structure
   of emotions, which specifies eliciting situations for each emotion and
   the variables that affect intensity. The emotional experiences in this
   structure are as follows: satisfaction, disappointment, hope, fear, joy,
   distress, boredom, interest, pride, reproach, self-reproach, like and
   dislike. These results show that the emotional experiences of students
   are based on their appraisals of events, objects and agents in terms of
   a structure of goals.},
