@article{ WOS:000936096900001,
Author = {Bitzenbauer, Philipp and Hoefler, Sebastian and Veith, Joaquin M. and
   Winkler, Bianca and Zenger, Tim and Kulgemeyer, Christoph},
Title = {Exploring the Relationship Between Surface Features and Explaining
   Quality of YouTube Explanatory Videos},
Journal = {INTERNATIONAL JOURNAL OF SCIENCE AND MATHEMATICS EDUCATION},
