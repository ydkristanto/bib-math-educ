@article{ WOS:000774645100001,
Author = {Xu, Lihua and Fang, Su-Chi and Hobbs, Linda},
Title = {The Relevance of STEM: a Case Study of an Australian Secondary School as
   an Arena of STEM Curriculum Innovation and Enactment},
