@article{ WOS:000538359500010,
Author = {Ross, Wendy and Vallee-Tourangeau, Frederic and Van Herwegen, Jo},
Title = {Mental Arithmetic and Interactivity: the Effect of Manipulating External
   Number Representations on Older Children's Mental Arithmetic Success},
Journal = {INTERNATIONAL JOURNAL OF SCIENCE AND MATHEMATICS EDUCATION},
