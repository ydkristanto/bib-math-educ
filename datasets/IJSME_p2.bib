@article{ WOS:000382937500007,
Author = {Ju, Mi-Kyung and Moon, Jong-Eun and Song, Ryoon-Jin},
Title = {History of Mathematics in Korean Mathematics Textbooks: Implication for
   Using Ethnomathematics in Culturally Diverse School},
Journal = {INTERNATIONAL JOURNAL OF SCIENCE AND MATHEMATICS EDUCATION},
Year = {2016},
Volume = {14},
Number = {7},
Pages = {1321-1338},
Month = {OCT},
Abstract = {From a multicultural perspective, this research investigated to what
   extent Korean mathematics textbooks use history of mathematics. The
   results show even though educational use of history presented in Korean
   mathematics textbooks may provide a rich outlook, it does not encourage
   a fundamental change in the educational practice of school mathematics
   that has traditionally been entrenched in the Eurocentric narrative of
   mathematics. This suggests that the mathematics textbooks were not
   organized effectively to promote students' understanding of diversity.
   Based on the results, we discuss the implications for the development of
   mathematics textbook from multicultural perspectives.},
