@article{ WOS:000683698800001,
Author = {Gal, Hagar},
Title = {Nurturing Autonomous Learners: from Small-Step Teaching to Generic
   Questions},
Journal = {INTERNATIONAL JOURNAL OF SCIENCE AND MATHEMATICS EDUCATION},
Year = {2022},
Volume = {20},
