@article{ WOS:000516016800003,
Author = {Bosser, Ulrika and Lindahl, Mats},
Title = {Teachers' Coordination of Dialogic and Authoritative Discourses
   Promoting Specific Goals in Socioscientific Issue-Based Teaching},
Journal = {INTERNATIONAL JOURNAL OF SCIENCE AND MATHEMATICS EDUCATION},
