@article{ WOS:000281009100004,
Author = {Cordero Osorio, Francisco and Cen Che, Claudia and Suarez Tellez,
   Liliana},
Title = {THE FUNCTIONINGS AND FORMS OF GRAPHS IN TEXTBOOKS: AN INSTITUTIONAL
   PRACTICE IN HIGH SCHOOL},
