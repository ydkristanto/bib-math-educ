@article{ WOS:000636606300005,
Author = {Montes, Miguel and Isabel Pascual, Ma and Climent, Nuria},
Title = {A TEACHING EXPERIMENT IN CONTINUING EDUCATION STRUCTURED BY THE MTSK
   MODEL},
Journal = {REVISTA LATINOAMERICANA DE INVESTIGACION EN MATEMATICA EDUCATIVA-RELIME},
Year = {2021},
Volume = {24},
Number = {1},
Pages = {83-104},
Month = {MAR},
Abstract = {The education of graduated teachers is an underexplored field in
   Mathematics Education. We present a teaching experiment focusing
   graduated teacher education, structured by the model of Mathematics
   Teachers' Specialized Knowledge. This experiment took place in the
   University of Huelva with 39 primary teachers, in the context of a
   course aiming to actualize the old teaching degrees to the new ones. The
   experiment was founded in the model of Mathematics Teachers Specialized
   Knowledge, that also was used to generate a progression hypothesis. We
   will show both the design of the experiment, and the retrospective
   analysis of this cycle. The results of the study show that the primary
   school teachers involved in the experiment improve their use of their
   knowledge of topics and knowledge of mathematics teaching.},
