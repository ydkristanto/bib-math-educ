@article{ WOS:000358898400004,
Author = {Garcia-Garcia, Javier and Rodriguez, Flor M. and Navarro, Catalina},
Title = {THE STRATEGIES USED BY CHILDREN <i>TEE SAVI</i> IN SOLVING ARITHMETIC
   PROBLEMS},
Journal = {REVISTA LATINOAMERICANA DE INVESTIGACION EN MATEMATICA EDUCATIVA-RELIME},
Year = {2015},
Volume = {18},
Number = {2},
Pages = {213-244},
Month = {JUL},
Abstract = {Mexico is a diversity country, culturally and linguistically speaking.
   Because of these and because the importance of interculturality proposed
   by the syllabus for mexican public education, we got interested in a
   particular community: the Tee Savi children (mixtecos). Particularly, we
   asked about the strategies they use into solving formal and practical
   arithmetic problems. Identifying those strategies allows us, among other
   things, to compare their efficiency in problem solving, specially on
   those kind of problems we mentioned before. We use the multiple case
   studies as research method, with a population composed by seventy
   students from primary school grades 4 degrees, 5 degrees and 6 degrees,
   their ages were from 9 to 13 years old. Questionnaires, written in
   spanish, and interviews, speaking in Tu'un Savi (students' mother
   language), were used as data collectors. The written and spoken
   evidences show the differences between the strategies used for each kind
   of problems.},
