@article{ WOS:000264786900004,
Author = {Jankvist, Uffe Thomas},
Title = {ON EMPIRICAL RESEARCH IN THE FIELD OF USING HISTORY IN MATHEMATICS
   EDUCATION},
Journal = {REVISTA LATINOAMERICANA DE INVESTIGACION EN MATEMATICA EDUCATIVA-RELIME},
Year = {2009},
Volume = {12},
Number = {1},
Pages = {67-101},
Month = {MAR},
Abstract = {This paper addresses the question of empirical research in the field of
   using history in Mathematics Education. More precisely, it focuses on
   what role empirical research may serve in the discussion of why to use
   history in mathematics education and how to do it. This is exemplified
   mainly by referring to two empirical research studies on the use of
   history in the Danish upper secondary mathematics program. Also it is
   illustrated how both the research design and research methodology of
   these two studies were dependant on the initial purpose of using history
   being concerned with history as a goal rather than history as a tool.
   Finally, perspectives are drawn on the possible benefits of increasing
   the amount of empirical research being done within the field of using
   history in mathematics education.},
