@article{ WOS:000322850300002,
Author = {Aravena Diaz, Maria and Caamano Espinoza, Carlos},
Title = {LEVELS OF GEOMETRIC REASONING IN STUDENTS OF STATAL SCHOOLS OF MAULE
   REGION. TALCA, CHILE},
Journal = {REVISTA LATINOAMERICANA DE INVESTIGACION EN MATEMATICA EDUCATIVA-RELIME},
Year = {2013},
Volume = {16},
Number = {2},
Pages = {139-178},
Month = {JUL},
Abstract = {In this research, financed by the National Fund for Scientific and
   Technological Development, FONDECYT 1090617, we analyze students'
   geometric reasoning from a municipal school, with high indices of
   vulnerability, in the region of Maule, Chile. Therefore, we made a
   diagnose test based on the Van-Hiele's model and Gutierrez \& Jaime's
   (1998) distinctive attributes in the reasoning process by considering
   the subjects students have from sixth grade, primary school, and from
   second grade, secondary education. The methodology was quantitative like
   where we took a sample by groups with a proportional allocation in a
   random sample. The effective sample was of 625 students which were
   representative of the area. Our main conclusion is that the students
   have a basic level of geometric reasoning and they have a low
   acquisition level in reasoning processes.},
