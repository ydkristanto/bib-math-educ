@article{ WOS:000976763600005,
Author = {Fonseca, Gorete and Da Ponte, Joao Pedro},
Title = {LESSON STUDY IN THE DEVELOPMENT OF KNOWLEDGE ABOUT TEACHING MATHEMATICS
   BY PRIMARY SCHOOL TEACHERS},
Journal = {REVISTA LATINOAMERICANA DE INVESTIGACION EN MATEMATICA EDUCATIVA-RELIME},
Year = {2022},
Volume = {25},
Number = {2},
Pages = {223-246},
Month = {JUL},
Abstract = {The aim of this article is to understand the learning of five primary
   teachers participating in a class study that focused on learning
   multiplication by 2nd year students with regard to the student's
   knowledge and learning processes and knowledge of teaching practice. It
   is a qualitative and interpretative investigation. Data collection was
   undertook by participant observation through the preparation of a
   research journal, audio and video recording of the sessions and
   conducting a semi-structured collective interview. The results confirm
   the potential of the LS by contributing to teachers to deepen their
   knowledge about the student's thinking and their strategies and
   difficulties in learning concepts. They also valued the detailed
   preparation of their action taking into account the previous knowledge
   of the students, as well as about exploratory learning.},
