@article{ WOS:000406945100003,
Author = {Gasco-Txabarri, Javier},
Title = {ARITHMETIC - ALGEBRAIC PROBLEM SOLVING AND LEARNING STRATEGIES IN MATHS.
   AN STUDY IN SECONDARY EDUCATION},
Journal = {REVISTA LATINOAMERICANA DE INVESTIGACION EN MATEMATICA EDUCATIVA-RELIME},
Year = {2017},
Volume = {20},
Number = {2},
Pages = {167-192},
Month = {JUL},
Abstract = {One of the essential blocks of teaching and learning mathematics
   consists of word problems and their resolution; learning strategies
   encourage autonomy and can help make decisions in this mathematical
   task. This study aims to relate the way to solve problems with the use
   of these strategies. The research is done with students of secondary
   school 8th, 9th and 10th grade. Students are categorized into three
   groups: the group of algebraic resolution, mixed resolution and the
   group without defined profile. Learning strategies are measured by a
   questionnaire. The algebraic group outperforms mixed in various
   strategies, especially in those metacognitives. The group without
   defined profile uses less all strategies except rehearsal.},
