@article{ WOS:000429122400005,
Author = {Zapatera Llinares, Alberto},
Title = {HOW PRIMARY EDUCATION STUDENTS SOLVE PROBLEMS OF GENERALIZATION OF
   PATTERNS. A LEARNING TRAJECTORY},
Journal = {REVISTA LATINOAMERICANA DE INVESTIGACION EN MATEMATICA EDUCATIVA-RELIME},
Year = {2018},
Volume = {21},
Number = {1},
Pages = {87-114},
Month = {MAR},
Abstract = {The objective of this research is to study the way in which students of
   Primary Education solve problems of generalization of patterns. For this
   we analyzed the level of success, the strategies used and progression of
   106 students in 3rd, 4th, 5th and 6th degree of Primary Education
   solving one problem of generalization of patterns. The results obtained
   show that the most successful students begin with additive strategies
   and then change to functional strategies, that reverse the process has a
   higher cognitive demand and very few students are able to express the
   general rule algebraically. The analysis of students' progression has
   allowed us to define a descriptors of learning trajectory that help to
   recognize the students' understanding and to describe their progress in
   terms of growth through ten levels of development.},
