@article{ WOS:000406945100004,
Author = {Mallart, Alberto and Deulofeu, Jordi},
Title = {MATHEMATICS CREATIVITY INDICATORS STUDY IN PROBLEM SOLVING},
Journal = {REVISTA LATINOAMERICANA DE INVESTIGACION EN MATEMATICA EDUCATIVA-RELIME},
Year = {2017},
Volume = {20},
Number = {2},
Pages = {193-222},
Month = {JUL},
Abstract = {Mathematics teaching has always been concerned about problem solving.
   The objective of this investigation is to determine the creativity
   degree when solving a mathematics problem. The University Admission Test
   of Mathematics subject of June of 2012 of Barcelona University has been
   chosen as the instrument to collect data. This Test contains 6 problems.
   From a sample of 104 students, seven creativity indicators have been
   analyzed for each resolution. The results show students are more
   creative when they design strategies than when they execute them or
   review the whole process; students distinguish all the parts of a
   resolution; but there is a poor capacity of communicating, organizing
   and synthesizing resolutions. Problem solving is a mathematics skill
   that has to be evaluated, and because of any resolution requires
   creative aspects, mathematical creativity has to be improved.},
