@article{ WOS:000264786900002,
Author = {Buendia, Gabriela and Ordonez, Alejandra},
Title = {PERIODIC BEHAVIOR IN THE RELATIONSHIP BETWEEN A FUNCTION AND ITS
   DERIVATIVES: MEANINGS FROM VARIATION},
Journal = {REVISTA LATINOAMERICANA DE INVESTIGACION EN MATEMATICA EDUCATIVA-RELIME},
Year = {2009},
Volume = {12},
Number = {1},
Pages = {7-28},
Month = {MAR},
Abstract = {This article deals with the relationship between a function and its
   derivatives for periodic functions. The aim is to provide elements that
   redefinition for the said relationship from its analysis in a context of
   variation and from a social practice perspective. We have studied some
   ways the periodic is used in the relationship between a function and its
   successive derivatives, in the contexts of movement and engineering
   where reference frameworks are noticeably wider than those considered in
   school discourse. It is by conducting intentional practices such as
   graphing, modeling or predicting that periodic behavior in function
   variations acquires meaning for scientific work.},
