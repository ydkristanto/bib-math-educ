@article{ WOS:000264786900003,
Author = {Crespo Crespo, Cecilia and Maria Farfan, Rosa and Lezama, Javier},
Title = {SOME CHARACTERISTICS OF THE ARGUMENTATIONS AND THE MATHEMATICS IN
   SCENARIOS WITHOUT ARISTOTELIAN INFLUENCE},
Journal = {REVISTA LATINOAMERICANA DE INVESTIGACION EN MATEMATICA EDUCATIVA-RELIME},
Year = {2009},
Volume = {12},
Number = {1},
Pages = {29-66},
Month = {MAR},
Abstract = {This work shows, first, a characterization Of Cultural scenarios that
   did not have Aristotelian influence; hence its argumentations used are
   different than the ones originated in Greece. In such cultures some
   mathematical concepts were used in a way that took several centuries to
   be accepted in western thinking. This shows how culture affects forms of
   argumentation and posits the possibility of the development of
   mathematical concepts based on other thinking sources.
   Furthermore, this study describes some argumentation forms, present in
   the mathematical classroom, which are incorrect according to
   Aristotelian logic.},
