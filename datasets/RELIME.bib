Language = {Spanish},
Affiliation = {Salgado, FJA (Corresponding Author), Univ Melbourne, Melbourne, Vic, Australia.
   Salgado, Felipe J. Almuna, Univ Melbourne, Melbourne, Vic, Australia.},
DOI = {10.12802/relime.17.2031},
ISSN = {1665-2436},
EISSN = {2007-6819},
Keywords = {Mathematical problem context; Context familiarity; Students' performance},
Keywords-Plus = {WORD-PROBLEMS; PERFORMANCE; KNOWLEDGE},
Research-Areas = {Education \& Educational Research},
