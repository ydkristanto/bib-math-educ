@article{ WOS:A1997WQ58200003,
Author = {Senk, SL and Beckmann, CE and Thompson, DR},
Title = {Assessment and grading in high school mathematics classrooms},
Journal = {JOURNAL FOR RESEARCH IN MATHEMATICS EDUCATION},
Year = {1997},
Volume = {28},
Number = {2},
Pages = {187-215},
Month = {MAR},
Note = {Annual Meeting of the American-Educational-Research-Association,
   ATLANTA, GA, APR 12-16, 1993},
Organization = {Amer Educ Res Assoc},
Abstract = {The assessment and grading practices in 19 mathematics classes in 5 high
   schools in 3 states were studied. In each class the most frequently used
   assessment tools were tests and quizzes, with these determining about
   77\% of students' grades. In 12 classes other forms of assessment, such
   as written projects or interviews with students, were also used, with
   performance on such instruments counting for about 7\% of students'
   grades averaged across all 19 classes. Test items generally were low
   level, were stated without reference to a realistic context, involved
   very little reasoning, and were almost never open-ended. Most test items
   were either neutral or inactive with respect to technology. Written
   projects usually involved more complex analyses or applications than
   tests did. The teachers' knowledge and beliefs, as well as the content
   and textbook of the course, influenced the characteristics of test items
   and other assessment instruments. Only in geometry classes did
   standardized tests appear to influence assessment.},
