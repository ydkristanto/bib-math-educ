@article{ WOS:000994062900003,
Author = {Paoletti, Teo and Gantt, Allison L. and Corven, Julien},
Title = {A Local Instruction Theory for Emergent Graphical Shape Thinking: A
   Middle School Case Study},
Journal = {JOURNAL FOR RESEARCH IN MATHEMATICS EDUCATION},
Year = {2023},
Volume = {54},
Number = {3},
Pages = {202-224},
Month = {MAY},
Abstract = {Emergent graphical shape thinking (EGST) involves interpreting or
   constructing a graph as dynamically generated, which is useful across
   science, technology, engineering, and mathematics fields. Although
   evidence suggests that students as young as middle school can engage in
   EGST with support, other research indicates most college students and
   U.S. teachers do not spontaneously engage in such reasoning when
   potentially productive. We describe a local instruction theory (LIT) to
   support middle school students developing EGST as part of their graphing
   meanings. We then present a case study to show how two students engaged
   with a task sequence designed with the LIT in mind to develop meanings
   for EGST. This article illustrates general principles researchers and
   educators could use to promote students' graphing meanings.},
Publisher = {NATL COUNCIL TEACHERS MATHEMATICS-NCTM},
