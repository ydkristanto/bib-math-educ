@article{ WOS:000440877100004,
Author = {Darragh, Lisa},
Title = {Loving and Loathing: Portrayals of School Mathematics in Young Adult
   Fiction},
Journal = {JOURNAL FOR RESEARCH IN MATHEMATICS EDUCATION},
Year = {2018},
Volume = {49},
Number = {2},
Pages = {178-209},
Month = {MAR},
Abstract = {Images of mathematics and mathematicians are often negative and
   stereotyped. These portrayals may work to construct our impressions of
   mathematics and influence students' identity with and future
   participation in the subject. This study examined young adult fiction as
   a context in which school mathematics is portrayed and constructed. I
   used positioning theory and the notion of story lines to analyze a
   sample of 59 books. Portrayals of school mathematics within this sample
   involved multiple story lines, including school mathematics as being
   obligatory but not useful and mathematics classes as tense, terrible,
   difficult, and different but perhaps as places in which to find love.
   Portrayals of mathematics teachers were extremely stereotyped, and some
   girls were just as likely as boys to be positioned as able mathematics
   learners.},
