@article{ WOS:A1997YF30800005,
Author = {Gutstein, E and Lipman, P and Hernandez, P and delosReyes, R},
Title = {Culturally relevant mathematics teaching in a Mexican American context},
Journal = {JOURNAL FOR RESEARCH IN MATHEMATICS EDUCATION},
Year = {1997},
Volume = {28},
Number = {6},
Pages = {709-737},
Month = {DEC},
Abstract = {This article examines mathematics instruction and its intersection with
   culturally relevant teaching in an elementary/middle school in a Mexican
   American community. The findings are based on a collaborative-research
   and school-change project involving university researchers, teachers,
   and the school's principal. On the basis of ethnographic data and an
   interdisciplinary theoretical framework, we propose a three-part model
   of culturally relevant mathematics instruction. The 3 components are (a)
   building on students' informal mathematical knowledge and building on
   students' cultural and experiential knowledge, (b) developing tools of
   critical mathematical thinking and critical thinking about knowledge in
   general, and (c) orientations to students' culture and experience.},
