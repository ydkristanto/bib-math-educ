@article{ WOS:000255375300005,
Author = {Van Dooren, Wim and De Bock, Dirk and Janssens, Dirk and Verschaffel,
   Lieven},
Title = {The linear imperative: An inventory and conceptual analysis of students'
   overuse of linearity},
Journal = {JOURNAL FOR RESEARCH IN MATHEMATICS EDUCATION},
Year = {2008},
Volume = {39},
Number = {3},
Pages = {311-342},
Month = {MAY},
Abstract = {The overreliance on linear methods in students' reasoning and problem
   solving has been documented and discussed by several scholars in the
   field. So far, however, there have been no attempts to assemble the
   evidence and to analyze it in a systematic way. This article provides an
   overview and a conceptual analysis of students' tendency to use linear
   methods beyond their applicability range. We start from an analysis of
   the linearity concept and its properties and representations. The main
   part consists of a summary and discussion of unwarranted applications of
   each of these properties and representations in various mathematical
   content domains in order to unravel similarities and differences. After
   that, we speculate on the factors that seem to be at the roots of the
   occurrence and persistence of the overreliance on linearity. Finally, we
   discuss educational implications and suggest perspectives for further
   research.},
Publisher = {NATL COUNCIL TEACHERS MATHEMATICS-NCTM},
Address = {1906 ASSOCIATION DRIVE, RESTON, VA 22091 USA},
Type = {Review},
Language = {English},
Affiliation = {De Bock, Dirk, Univ Leuven, EHSAL European Univ Coll Brussels, B-3000 Louvain, Belgium.
   Janssens, Dirk, Univ Leuven, Dept Math, B-3001 Heverlee, Belgium.
   Verschaffel, Lieven, Univ Leuven, Ctr Instruct Psychol \& Technol, B-3000 Louvain, Belgium.},
DOI = {10.2307/30034972},
