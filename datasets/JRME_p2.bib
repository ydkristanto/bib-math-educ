@article{ WOS:001107817500003,
Author = {Resnick, Ilyse and Lowrie, Tom},
Title = {Spatial Reasoning Supports Preschool Numeracy: Findings From a
   Large-Scale Nationally Representative Randomized Control Trial},
Journal = {JOURNAL FOR RESEARCH IN MATHEMATICS EDUCATION},
Year = {2023},
Volume = {54},
Number = {5},
Pages = {295-316},
Month = {NOV},
Abstract = {We assessed the efficacy of two spatial learning programs grounded in
   early years learning pedagogical theory to improve numeracy performance
   in preschool. Engagement with a play-based spatial program led to better
   overall spatial reasoning and transferred to better numeracy compared
   with a business-as-usual control, underscoring the importance of
   embedding spatial learning within strong pedagogy and authentic
   preschool contexts. Engagement with the same spatial program using a
   spatialized curriculum (e.g., gesture, sketching) showed large additive
   effects, highlighting the role of spatial reasoning tools to support
   transfer of spatial reasoning to numeracy. The effects of the two
   interventions were moderated by spatial reasoning, with children with
   lower spatial reasoning making the most gains in numeracy.},
Publisher = {NATL COUNCIL TEACHERS MATHEMATICS-NCTM},
Address = {1906 ASSOCIATION DRIVE, RESTON, VA 22091 USA},
Type = {Article},
Language = {English},
Affiliation = {Resnick, I (Corresponding Author), Univ Canberra, STEM Educ Res Ctr, Bruce, ACT 2617, Australia.
   Resnick, Ilyse; Lowrie, Tom, Univ Canberra, STEM Educ Res Ctr, Bruce, ACT 2617, Australia.},
DOI = {10.5951/jresematheduc-2022-0051},
ISSN = {0021-8251},
EISSN = {1945-2306},
