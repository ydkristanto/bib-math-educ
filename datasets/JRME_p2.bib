@article{ WOS:000081260600001,
Author = {Schwarz, BB and Hershkowitz, R},
Title = {Prototypes: Brakes or levers in learning the function concept? The role
   of computer tools},
Journal = {JOURNAL FOR RESEARCH IN MATHEMATICS EDUCATION},
Year = {1999},
Volume = {30},
Number = {4},
Pages = {362-389},
Month = {JUL},
Abstract = {Prototypes are used as references to cope with new examples in concept
   learning. Prototypes can, however, he detrimental to concept learning,
   as shown in the use of linear functions for learning the function
   concept. This research characterizes students' function concept images
   that arise in an interactive environment based on multirepresentational
   software. We capitalize on a 20-year curricular program to contrast the
