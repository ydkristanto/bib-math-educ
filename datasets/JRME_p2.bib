@article{ WOS:000347757900004,
Author = {Lewis, Katherine E.},
Title = {Difference Not Deficit: Reconceptualizing Mathematical Learning
   Disabilities},
Journal = {JOURNAL FOR RESEARCH IN MATHEMATICS EDUCATION},
Year = {2014},
Volume = {45},
Number = {3},
Pages = {351-396},
Month = {MAY},
Abstract = {Mathematical learning disability (MLD) research often conflates low
   achievement with disabilities and focuses exclusively on deficits of
   students with MLDs. In this study I adopt an alternative approach using
   a response-to-intervention MLD classification model and identify the
   resources students draw upon rather than the skills they lack. The
   intervention model involved videotaped one-on-one fraction tutoring
   sessions implemented with students with low mathematics achievement.
   This article presents case studies of two students who did not benefit
   from the tutoring sessions. Detailed diagnostic analyses of the sessions
   revealed that the students understood mathematical representations in
   atypical ways and that this directly contributed to the persistent
   difficulties they experienced. Implications for screening and
   remediation approaches are discussed.},
