@article{ WOS:000265548500005,
Author = {Ng, Swee Fong and Lee, Kerry},
Title = {The Model Method: Singapore Children's Tool for Representing and Solving
   Algebraic Word Problems},
Journal = {JOURNAL FOR RESEARCH IN MATHEMATICS EDUCATION},
Year = {2009},
Volume = {40},
Number = {3},
Pages = {282-313},
Month = {MAY},
Abstract = {Solving arithmetic and algebraic word problems is a key component of the
   Singapore elementary mathematics curriculum. One heuristic taught, the
   model method, involves drawing a diagram to represent key information in
   the problem. We describe the model method and a three-phase theoretical
   framework supporting its use. We conducted 2 studies to examine
   teachers' perceptions and children's application of the model method.
   The subjects were 14 primary teachers from 4 schools and 151 Primary 5
   children. The model method affords higher ability children without
   access to letter-symbolic algebra a means to represent and solve
   algebraic word problems. Partly correct solutions suggest that
   representation is not an all-or-nothing process in which model drawing
   is either completely correct or completely incorrect. Instead, an
   incorrect solution could be the consequence of misrepresentation of a
   single piece of information. Our findings offer avenues of support in
   word problem solving to children of average ability.},
