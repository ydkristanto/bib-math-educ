@article{ WOS:000177812500005,
Author = {Ho, HZ and Senturk, D and Lam, AG and Zimmer, JM and Hong, S and
   Okamoto, Y and Chiu, SY and Nakazawa, Y and Wang, CP},
Title = {The affective and cognitive dimensions of math anxiety: A cross-national
   study},
Journal = {JOURNAL FOR RESEARCH IN MATHEMATICS EDUCATION},
Year = {2000},
Volume = {31},
Number = {3},
Pages = {362-379},
Month = {MAY},
Abstract = {In this study we focus on math anxiety, comparing its dimensions,
   levels, and relationship with mathematics achievement across samples of
   6th-grade student!, from China. Taiwan, and the United States. The
   results of confirmatory factor analyses supported the theoretical
   distinction between affective and cognitive dimensions of math anxiety
   in all 3 national samples. The analyses of structural equation models
   provided evidence for the differential predictive validity of the 2
   dimensions of math anxiety. Specifically, across the 3 national samples,
   the affective factor of math anxiety was significantly related to
   mathematics achievement in the negative direction. Gender by nation
   interactions were also found to be significant for both affective and
