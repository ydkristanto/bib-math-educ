@article{ WOS:000520655500001,
Author = {Paliwal, Veena and Baroody, Arthur J.},
Title = {Cardinality principle understanding: the role of focusing on the
   subitizing ability},
Journal = {ZDM-MATHEMATICS EDUCATION},
Year = {2020},
Volume = {52},
