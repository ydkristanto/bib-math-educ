   de Freitas, Elizabeth, Manchester Metropolitan Univ, Manchester, Lancs, England.
   Ferrara, Francesca; Ferrari, Giulia, Univ Torino, Turin, Italy.},
DOI = {10.1007/s11858-018-1007-4},
ISSN = {1863-9690},
EISSN = {1863-9704},
Keywords = {Mathematical tasks; Movement; Affect; Emotion; Sympathy; Collaborative
