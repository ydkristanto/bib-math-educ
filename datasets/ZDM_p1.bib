@article{ WOS:001119305500001,
Author = {Engelbrecht, Johann and Borba, Marcelo C.},
Title = {Recent developments in using digital technology in mathematics education},
Journal = {ZDM-MATHEMATICS EDUCATION},
Year = {2023},
Month = {2023 NOV 18},
Abstract = {In this paper we review selected significant developments in the use of
   digital technology in the teaching and learning of mathematics over the
   last five years. We focus on a number of important topics in this field,
   including the evolvement of STEAM and critical making as well as the
   process of redefining learning spaces in the transformation of the
   mathematics classroom. We also address the increasing use of computer
   algebra systems and dynamic geometry packages; and the issue of student
   collaboration online, especially using learning environments and social
   media. We briefly touch on artificial intelligence systems, including
   hyper-personalisation of learning, multimodality and videos. We include
   a brief discussion on the impact of COVID-19 on mathematics education,
   and lastly on the more theoretical perspective of the epistemology of
   digital technology and the construct of humans-with-media. We conclude
   the discussion with some possible concerns and mentioning some possible
   new topics for research in the field.},
