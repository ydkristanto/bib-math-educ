@article{ WOS:000881545800002,
Author = {Graven, Mellony and Venkat, Hamsa and Bowie, Lynn},
Title = {Analysing the citizenship agenda in Mathematical Literacy school exit
   assessments},
Journal = {ZDM-MATHEMATICS EDUCATION},
