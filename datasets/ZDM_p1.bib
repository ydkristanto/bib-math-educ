@article{ WOS:000463647400015,
Author = {Mellone, Maria and Ramploud, Alessandro and Di Paola, Benedetto and
   Martignone, Francesca},
Title = {Cultural transposition: Italian didactic experiences inspired by Chinese
   and Russian perspectives on whole number arithmetic},
Journal = {ZDM-MATHEMATICS EDUCATION},
Year = {2019},
Volume = {51},
Number = {1, SI},
Pages = {199-212},
Month = {APR},
Abstract = {The paper presents some reflections and activities developed by
   researchers and teachers involved in teacher education programs on
   cultural transposition. The construct of cultural transposition is
   presented as a condition for decentralizing the didactic practice of a
   specific cultural context through contact with other didactic practices
   of different cultural contexts. We discuss the background theoretical
   issues of this approach and also give an analysis of two examples of
   cultural transposition experiences carried out in Italy. In particular,
   by means of qualitative analysis of some excerpts, discussions, and
   interviews, we show that the contact with different perspectives coming
   from China and Russia fostered educational practices and reflections on
   whole number arithmetic education.},
