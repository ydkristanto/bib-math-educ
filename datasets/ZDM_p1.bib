@article{ WOS:000510836600012,
Author = {Dogan, Hamide},
Title = {Some aspects of linear independence schemas},
Journal = {ZDM-MATHEMATICS EDUCATION},
Year = {2019},
Volume = {51},
Number = {7, SI},
Pages = {1169-1181},
Month = {DEC},
Abstract = {In this study, I examined seven first-year linear algebra students'
   linear independence schemas. Data came from participants' interview
   responses to a set of nine questions. The analysis focused on the
   identification of concepts and connections pertaining to plans and
   activations. Overall, the findings revealed the existence of routinized
   plans, each containing one of six most frequently expressed connections.
   Moreover, I observed some participants activating these plans as fixed
   plans, and other participants activating them as ready-to-hand plans.
   Some activations were task specific. In short, I found participants'
   linear independence schemas to be populated by three main routinized
   plans, each with varying characteristics.},
