@article{ WOS:001066746700001,
Author = {Foley, Gregory D. and Budhathoki, Deependra and Thapa, Amrit B. and
   Aryal, Harman P.},
Title = {Instructor perspectives on quantitative reasoning for critical
   citizenship},
Journal = {ZDM-MATHEMATICS EDUCATION},
