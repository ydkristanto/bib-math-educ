@article{ WOS:000626371800001,
Author = {Lyublinskaya, Irina and Petrova, Elena},
Title = {Mathematics learning in physics classrooms of Russian schools: a
   changing landscape from the Soviet period to the present},
Journal = {ZDM-MATHEMATICS EDUCATION},
Year = {2021},
Volume = {53},
Number = {7, SI},
Pages = {1485-1498},
Month = {DEC},
Abstract = {For a long time, Soviet students learned `pure' mathematics in their
   mathematics classrooms, while applications of mathematics were
   introduced in their science (mainly physics) classrooms. This approach
   was a part of a uniform and rigid national curriculum. Even when in the
   1990s the world was moving towards including applications in school
   mathematics, Russian students continued to engage in pure mathematics
   learning in their mathematics classrooms. It was physics teachers'
   responsibility to teach applications of mathematics; therefore, physics
   courses were highly mathematics-intensive, making extensive use of
   mathematics from algebra to calculus in the formulation of scientific
   laws and the investigation of their consequences. The collapse of the
   Soviet Union and some liberalization in educational policy led to
   changes in graduation requirements in mathematics and physics as well as
   diversity in mathematics and physics curricula in schools. Based on a
   review of textbooks, standards, curriculum documents, and other
   resources, in this paper we analyze changes that affected the teaching
   and learning of mathematics in physics classrooms in Russia from the
   Soviet period to the present.},
Publisher = {SPRINGER HEIDELBERG},
Address = {TIERGARTENSTRASSE 17, D-69121 HEIDELBERG, GERMANY},
Type = {Article},
Language = {English},
Affiliation = {Lyublinskaya, I (Corresponding Author), Columbia Univ, Teachers Coll, New York, NY 10027 USA.
   Lyublinskaya, Irina, Columbia Univ, Teachers Coll, New York, NY 10027 USA.
   Petrova, Elena, Moscow State Pedag Univ, Moscow, Russia.},
DOI = {10.1007/s11858-021-01248-z},
EarlyAccessDate = {MAR 2021},
ISSN = {1863-9690},
EISSN = {1863-9704},
Keywords = {Pure mathematics; Applied mathematics; Curriculum; \&\#8216; Mathematics
