@article{ WOS:000449300600010,
Author = {Biehler, Rolf and Frischemeier, Daniel and Podworny, Susanne},
Title = {Elementary preservice teachers' reasoning about statistical modeling in
   a civic statistics context},
Journal = {ZDM-MATHEMATICS EDUCATION},
Year = {2018},
Volume = {50},
Number = {7, SI},
Pages = {1237-1251},
Month = {DEC},
Abstract = {Elements of statistical modeling can be implemented already in primary
   school. A prerequisite for this approach is that teachers are
   well-educated in this domain. Content knowledge, pedagogical content
   knowledge and (pedagogical) content related technological knowledge are
   core components of teacher education. We designed a course for
   elementary preservice teachers with regard to developing statistical
   thinking including the mentioned knowledge facets. The course includes
   exploring data and modeling and simulating chance experiments with
   TinkerPlots. We use the data factory metaphor' in fictive contexts and
   in contexts stemming from civic statistics for supporting the idea of
   modeling. We interviewed four participants of the course to assess and
   analyze their reasoning. We analyze how they model a given civic
   statistics contextual problem using the TinkerPlots sampler and how they
   evaluate their model with regard to a civic statistics context (the
   situation of hospitals in Germany).},
