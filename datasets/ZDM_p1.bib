@article{ WOS:000414211000006,
Author = {Niu, Weihua and Zhou, Zheng and Zhou, Xinlin},
Title = {Understanding the Chinese approach to creative teaching in mathematics
   classrooms},
Journal = {ZDM-MATHEMATICS EDUCATION},
Year = {2017},
Volume = {49},
Number = {7},
Pages = {1023-1031},
Month = {DEC},
Abstract = {Using Amabile's componential theory of creativity as a framework, this
   paper analyzes how Chinese mathematics teachers achieve creative
   teaching through acquiring in-depth domain-specific knowledge in
   mathematics, developing creativity-related skills, as well as
   stimulating student interest in learning mathematics, through
   well-crafted, activity-enriched lessons. It argues that creative
   mathematics teaching in the Chinese context is reference-based. There is
   a standard on what good teaching should look like and there is a greater
   emphasis on appropriateness in addition to novelty. Chinese mathematics
   classrooms are also mostly teacher-led, and Chinese teachers often spend
   a great deal of time to harness their creative teaching methods through
   various levels of professional development. The mechanism of exemplary
   mathematics lesson contest fosters collaborations among teachers on
   course preparation to achieve more effective mathematics teaching. Both
   features are consistent with the Chinese view of creativity.
   Implications are discussed.},
